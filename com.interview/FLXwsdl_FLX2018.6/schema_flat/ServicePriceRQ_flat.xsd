              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="UserInput" type="UserInputResponseType">
              <xsd:annotation>
                <xsd:documentation source="DESCRIPTION" xml:lang="en">User input required for this service.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="Endorsement">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Endorsement information.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="Text" type="FreeFormTextType">
              <xsd:annotation>
                <xsd:documentation source="DESCRIPTION" xml:lang="en">Endorsement text.
Example: NONREF</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" ref="Penalties" />
      <xsd:element minOccurs="0" ref="Commission" />
      <xsd:element minOccurs="0" ref="Combinality" />
      <xsd:element minOccurs="0" ref="IncentiveData" />
      <xsd:element minOccurs="0" ref="Diagnostics" />
      <xsd:element minOccurs="0" ref="Attributes" />
      <xsd:element minOccurs="0" ref="MediaReference" />
    </xsd:sequence>
    <xsd:attributeGroup ref="SrvcItemAttributesGroup" />
  </xsd:complexType>
  <xsd:complexType name="ServiceIncentiveDataType">
    <xsd:annotation>
      <xsd:documentation source="DESCRIPTION" xml:lang="en">Incentive information.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element maxOccurs="unbounded" name="Offer">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Offer detail.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="OfferCodeID">
              <xsd:annotation>
                <xsd:documentation source="DESCRIPTION" xml:lang="en">Rules engine offer ID.
Example: 0AR123</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:simpleContent>
                  <xsd:extension base="xsd:string">
                    <xsd:attribute name="OfferSubCode" type="xsd:string" use="optional">
                      <xsd:annotation>
                        <xsd:documentation source="DESCRIPTION" xml:lang="en">Offer subcode generated on creation by SQL.</xsd:documentation>
                      </xsd:annotation>
                    </xsd:attribute>
                  </xsd:extension>
                </xsd:simpleContent>
              </xsd:complexType>
            </xsd:element>
            <xsd:element minOccurs="0" name="AvailableUnits" type="xsd:integer">
              <xsd:annotation>
                <xsd:documentation source="DESCRIPTION" xml:lang="en">Quantity of available units.
Example: 4</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="ExpirationDate" type="DateTimeType">
              <xsd:annotation>
                <xsd:documentation source="DESCRIPTION" xml:lang="en">Offer expiration date.
Example: 2013-01-13</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="DiscountLevel">
              <xsd:annotation>
                <xsd:documentation source="DESCRIPTION" xml:lang="en">Discount level value.
Example: 20</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:simpleContent>
                  <xsd:extension base="xsd:string">
                    <xsd:attribute name="Type" type="AmountCategoryType" use="required">
                      <xsd:annotation>
                        <xsd:documentation source="DESCRIPTION" xml:lang="en">Unit of measure.
Example: A (AMOUNT)</xsd:documentation>
                        <xsd:documentation source="ENCODING" xml:lang="en">=enumerated values=
A = AMOUNT (CURRENCY)
P = PERCENTAGE</xsd:documentation>
                      </xsd:annotation>
                    </xsd:attribute>
                  </xsd:extension>
                </xsd:simpleContent>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="ServiceCombineType">
    <xsd:annotation>
      <xsd:documentation source="DESCRIPTION" xml:lang="en">Identifies how offers may be combined.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element maxOccurs="unbounded" name="ServiceItem">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Services detail.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:choice>
            <xsd:element minOccurs="0" ref="ServiceID" />
            <xsd:element minOccurs="0" ref="OfferID" />
          </xsd:choice>
          <xsd:attribute name="CombineInd" type="ChoiceType" use="required">
            <xsd:annotation>
              <xsd:documentation source="DESCRIPTION" xml:lang="en">If Y, this service MAY BE COMBINED with the specified service.
If N, this service MAY NOT BE COMBINED with the specified service.
Example: Y (YES)</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute ref="ReasonCode" use="optional" />
          <xsd:attribute ref="ServiceType" use="optional" />
          <xsd:attribute ref="ServiceCode" use="optional" />
          <xsd:attribute ref="SubCode" use="optional" />
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="ServicePenaltyType">
    <xsd:annotation>
      <xsd:documentation source="DESCRIPTION" xml:lang="en">Penalty information.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="FeeApplies">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Applicable fee information.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:attribute default="N" name="ChangeItinerary" type="ChoiceType" use="optional">
            <xsd:annotation>
              <xsd:documentation source="DESCRIPTION" xml:lang="en">If Y, a change of itinerary fee applies.
Example: N (NO)</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute default="N" name="Cancellation" type="ChoiceType" use="optional">
            <xsd:annotation>
              <xsd:documentation source="DESCRIPTION" xml:lang="en">If Y, cancellation fees apply.
Example: Y (YES)</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="3" name="Change">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Change penalty information.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element maxOccurs="2" name="Amount">
              <xsd:annotation>
                <xsd:documentation source="DESCRIPTION" xml:lang="en">Currency or percentage amount.
Example: 7500</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:simpleContent>
                  <xsd:extension base="xsd:integer">
                    <xsd:attribute name="Unit" type="AmountCategoryType" use="required">
                      <xsd:annotation>
                        <xsd:documentation source="DESCRIPTION" xml:lang="en">Unit of measure.
Example:
A (AMOUNT)
P (PERCENTAGE)</xsd:documentation>
                        <xsd:documentation source="ENCODING" xml:lang="en">Select a value from the enumerated list.
=enumerated values=
A = CURRENCY AMOUNT
P = PERCENTAGE</xsd:documentation>
                      </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="MinMax" use="optional">
                      <xsd:annotation>
                        <xsd:documentation source="DESCRIPTION" xml:lang="en">Minimum or maximum penalty indicator.
Example:
MIN (MINIMUM PENALTY AMOUNT)
MAX MAXIMUM PENALTY AMOUNT)</xsd:documentation>
                        <xsd:documentation source="ENCODING" xml:lang="en">=enumerated values=
MIN = MINIMUM PENALTY AMOUNT
MAX = MAXIMUM PENALTY AMOUNT</xsd:documentation>
                      </xsd:annotation>
                      <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                          <xsd:minLength value="1" />
                          <xsd:maxLength value="1" />
                          <xsd:enumeration value="MIN">
                            <xsd:annotation>
                              <xsd:documentation>MIN = MINIMUM PENALTY AMOUNT</xsd:documentation>
                            </xsd:annotation>
                          </xsd:enumeration>
                          <xsd:enumeration value="MAX">
                            <xsd:annotation>
                              <xsd:documentation>MAX = MAXIMUM PENALTY AMOUNT</xsd:documentation>
                            </xsd:annotation>
                          </xsd:enumeration>
                        </xsd:restriction>
                      </xsd:simpleType>
                    </xsd:attribute>
                  </xsd:extension>
                </xsd:simpleContent>
              </xsd:complexType>
            </xsd:element>
            <xsd:element minOccurs="0" maxOccurs="unbounded" name="Text" type="FreeFormTextType" />
            <xsd:element minOccurs="0" name="Taxes" type="TaxesType" />
          </xsd:sequence>
          <xsd:attribute name="Application" use="optional">
            <xsd:annotation>
              <xsd:documentation source="DESCRIPTION" xml:lang="en">Penalty application type.
Examples:
ADE (After departure)
NOS (No show)
PDE (Prior to departure)

For maximum interoperability, refer to IATA PADIS Code List FAT - Fare or Application Penalty Type. Select a code value or enter a value that is bilaterally agreed upon between trading partners.</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction base="xsd:string" />
            </xsd:simpleType>
          </xsd:attribute>
          <xsd:attribute name="Total" type="xsd:integer" use="required">
            <xsd:annotation>
              <xsd:documentation>Total Change fee Taxes + Total Change Fee amount</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Amount" type="xsd:integer" use="required">
            <xsd:annotation>
              <xsd:documentation>Then change fee amount to be collected as penalty</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="3" name="Cancel">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Cancel penalty information.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element maxOccurs="2" name="Amount">
              <xsd:annotation>
                <xsd:documentation source="DESCRIPTION" xml:lang="en">Currency or percentage amount.
Example: 7500</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:simpleContent>
                  <xsd:extension base="xsd:integer">
                    <xsd:attribute name="Unit" type="AmountCategoryType" use="required">
                      <xsd:annotation>
                        <xsd:documentation source="DESCRIPTION" xml:lang="en">Unit of measure.
Example:
A (AMOUNT)
P (PERCENTAGE)</xsd:documentation>
                        <xsd:documentation source="ENCODING" xml:lang="en">Select a value from the enumerated list.
=enumerated values=
A = CURRENCY AMOUNT
P = PERCENTAGE</xsd:documentation>
                      </xsd:annotation>
                    </xsd:attribute>
                    <xsd:attribute name="MinMax" use="optional">
                      <xsd:annotation>
                        <xsd:documentation source="DESCRIPTION" xml:lang="en">Minimum or maximum penalty indicator.
Example:
MIN (MINIMUM PENALTY AMOUNT)
MAX MAXIMUM PENALTY AMOUNT)</xsd:documentation>
                        <xsd:documentation source="ENCODING" xml:lang="en">=enumerated values=
MIN = MINIMUM PENALTY AMOUNT
MAX = MAXIMUM PENALTY AMOUNT</xsd:documentation>
                      </xsd:annotation>
                      <xsd:simpleType>
                        <xsd:restriction base="xsd:string">
                          <xsd:minLength value="1" />
                          <xsd:maxLength value="1" />
                          <xsd:enumeration value="MIN">
                            <xsd:annotation>
                              <xsd:documentation>MIN = MINIMUM PENALTY AMOUNT</xsd:documentation>
                            </xsd:annotation>
                          </xsd:enumeration>
                          <xsd:enumeration value="MAX">
                            <xsd:annotation>
                              <xsd:documentation>MAX = MAXIMUM PENALTY AMOUNT</xsd:documentation>
                            </xsd:annotation>
                          </xsd:enumeration>
                        </xsd:restriction>
                      </xsd:simpleType>
                    </xsd:attribute>
                  </xsd:extension>
                </xsd:simpleContent>
              </xsd:complexType>
            </xsd:element>
            <xsd:element minOccurs="0" maxOccurs="unbounded" name="Text" type="FreeFormTextType" />
            <xsd:element minOccurs="0" name="Taxes" type="TaxesType" />
          </xsd:sequence>
          <xsd:attribute name="Application" use="optional">
            <xsd:annotation>
              <xsd:documentation source="DESCRIPTION" xml:lang="en">Penalty application type.
Examples:
ADE (After departure)
NOS (No show)
PDE (Prior to departure)

For maximum interoperability, refer to IATA PADIS Code List FAT - Fare or Application Penalty Type. Select a code value or enter a value that is bilaterally agreed upon between trading partners.</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction base="xsd:string">
                <xsd:enumeration value="PRIOR_DEP">
                  <xsd:annotation>
                    <xsd:documentation>PRIOR_DEP = PRIOR TO DEPARTURE</xsd:documentation>
                  </xsd:annotation>
                </xsd:enumeration>
                <xsd:enumeration value="AFTER_DEP">
                  <xsd:annotation>
                    <xsd:documentation>AFTER_DEP = AFTER DEPARTURE</xsd:documentation>
                  </xsd:annotation>
                </xsd:enumeration>
                <xsd:enumeration value="NO_SHOW">
                  <xsd:annotation>
                    <xsd:documentation>NO_SHOW = NO SHOW</xsd:documentation>
                  </xsd:annotation>
                </xsd:enumeration>
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:attribute>
          <xsd:attribute name="Total" type="xsd:integer" use="required">
            <xsd:annotation>
              <xsd:documentation>Total Change fee Taxes + Total Change Fee amount</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Amount" type="xsd:integer" use="required">
            <xsd:annotation>
              <xsd:documentation>Then change fee amount to be collected as penalty</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Refundable" type="ChoiceType" use="optional">
      <xsd:annotation>
        <xsd:documentation source="DESCRIPTION" xml:lang="en">If Y, this is a refundable ticket.
Example: Y (YES)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="Reusable" type="ChoiceType" use="optional">
      <xsd:annotation>
        <xsd:documentation source="DESCRIPTION" xml:lang="en">If Y, this is a reusable fare.
Example: Y (YES)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="ServicePriceType">
    <xsd:annotation>
      <xsd:documentation source="DESCRIPTION" xml:lang="en">Optional service PRICING information.</xsd:documentation>
      <xsd:documentation source="GUIDELINES" xml:lang="en">=notes=
1. IMPORTANT:  This is NOT used for "included" (no charge) options.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="CurrencyCode" type="CurrencyCodeType">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Currency code.
Example: USD</xsd:documentation>
          <xsd:documentation source="ENCODING" xml:lang="en">For maximum interoperability, all currency codes are specified  in ISO 4217 format.
http://www.iso.org/iso/home/standards/iso8601.htm</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="BasePrice" type="ServiceBasePriceType">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Base (net) price information.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Taxes" type="TaxesType">
        <xsd:annotation>
          <xsd:documentation>Tax information.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ExemptedTaxes" type="ExemptTaxType">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Exempt tax information.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:choice minOccurs="0">
        <xsd:element minOccurs="0" ref="Redemption" />
        <xsd:element minOccurs="0" ref="RedemptionCurrency" />
      </xsd:choice>
      <xsd:element minOccurs="0" name="ServicePriceCalculation">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Dynamic service price calculation information.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="0" maxOccurs="unbounded" name="Operation">
              <xsd:annotation>
                <xsd:documentation source="DESCRIPTION" xml:lang="en">Calculation operation(s) detail.</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element name="Adjustment">
                    <xsd:annotation>
                      <xsd:documentation source="DESCRIPTION" xml:lang="en">Actual adjustment amount value.
Example: 12.50</xsd:documentation>
                      <xsd:documentation source="GUIDELINES" xml:lang="en">=notes=
1. This is the adjustment amount made representing the actual adjustment amount.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                      <xsd:simpleContent>
                        <xsd:extension base="xsd:string">
                          <xsd:attribute name="Type" type="SrvcPriceAdjType" use="required">
                            <xsd:annotation>
                              <xsd:documentation source="DESCRIPTION" xml:lang="en">Adjustment type.
Example: DIS (DISCOUNT)

For maximum interoperability, Refer to IATA PADIS code list SPA - Service Price Adjustment. Select a code value or enter a value that is bilaterally agreed upon between trading partners.</xsd:documentation>
                            </xsd:annotation>
                          </xsd:attribute>
                          <xsd:attribute name="Percentage" type="xsd:string" use="optional">
                            <xsd:annotation>
                              <xsd:documentation source="DESCRIPTION" xml:lang="en">Calculation percentage value.
Example: 12.5</xsd:documentation>
                            </xsd:annotation>
                          </xsd:attribute>
                          <xsd:attribute name="Input" type="xsd:decimal" use="required">
                            <xsd:annotation>
                              <xsd:documentation source="DESCRIPTION" xml:lang="en">Actual price PRIOR to calculation.
Example: 75.00</xsd:documentation>
                            </xsd:annotation>
                          </xsd:attribute>
                          <xsd:attribute name="Result" type="xsd:decimal" use="required">
                            <xsd:annotation>
                              <xsd:documentation source="DESCRIPTION" xml:lang="en">New price AFTER completed calculation.
Example: 82.50</xsd:documentation>
                            </xsd:annotation>
                          </xsd:attribute>
                        </xsd:extension>
                      </xsd:simpleContent>
                    </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="Designator" type="xsd:string">
                    <xsd:annotation>
                      <xsd:documentation source="DESCRIPTION" xml:lang="en">Unique designator for this adjustment.
Example: A3</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element minOccurs="0" name="Description" type="xsd:string">
                    <xsd:annotation>
                      <xsd:documentation source="DESCRIPTION" xml:lang="en">Summary (one line) event description.</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element minOccurs="0" name="DescriptionDetails">
                    <xsd:annotation>
                      <xsd:documentation source="DESCRIPTION" xml:lang="en">Detailed event description.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                      <xsd:sequence>
                        <xsd:element maxOccurs="unbounded" name="Text" type="xsd:string">
                          <xsd:annotation>
                            <xsd:documentation source="DESCRIPTION" xml:lang="en">Event description text.</xsd:documentation>
                          </xsd:annotation>
                        </xsd:element>
                      </xsd:sequence>
                    </xsd:complexType>
                  </xsd:element>
                  <xsd:element minOccurs="0" name="Reason" type="xsd:string">
                    <xsd:annotation>
                      <xsd:documentation source="DESCRIPTION" xml:lang="en">Adjustment reason or reason code.
Example: J45</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element minOccurs="0" name="Diag" type="xsd:string">
                    <xsd:annotation>
                      <xsd:documentation source="DESCRIPTION" xml:lang="en">Diagnostics ID.
Example: 234567</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                  <xsd:element minOccurs="0" name="RedemptionCurrency">
                    <xsd:annotation>
                      <xsd:documentation source="DESCRIPTION" xml:lang="en">Redemption currency, e.g. frequent flyer miles, conversion rate.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                      <xsd:attribute name="ConversionRatio" type="xsd:string" use="optional">
                        <xsd:annotation>
                          <xsd:documentation source="DESCRIPTION" xml:lang="en">Currency to alternate currency ratio.
Example: 1:20</xsd:documentation>
                        </xsd:annotation>
                      </xsd:attribute>
                    </xsd:complexType>
                  </xsd:element>
                </xsd:sequence>
                <xsd:attribute name="Seq" type="xsd:integer" use="required">
                  <xsd:annotation>
                    <xsd:documentation source="DESCRIPTION" xml:lang="en">Sequence calculation position.
Example: 3</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Total" type="xsd:integer" use="required">
      <xsd:annotation>
        <xsd:documentation source="DESCRIPTION" xml:lang="en">Total amount.
Example: 7500</xsd:documentation>
        <xsd:documentation source="GUIDELINES" xml:lang="en">=notes=
1. Calculation: Total = base price +  total amount of taxes
2. This is the total amount for this service based on FQTV status, chosen price class and other optional service qualifiers.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="RedemptionOnly" type="ChoiceType" use="optional">
      <xsd:annotation>
        <xsd:documentation source="DESCRIPTION" xml:lang="en">If Y, this service must be REDEEMED and cannot be purchased.
Example: Y (YES)

=notes=
1. Taxes may still be required.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="ServiceProcessInfoType">
    <xsd:annotation>
      <xsd:documentation source="DESCRIPTION" xml:lang="en">Optional service-level processing information.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:sequence>
        <xsd:element maxOccurs="unbounded" name="Text">
          <xsd:annotation>
            <xsd:documentation source="DESCRIPTION" xml:lang="en">Information text.</xsd:documentation>
          </xsd:annotation>
          <xsd:simpleType>
            <xsd:restriction base="xsd:string">
              <xsd:minLength value="1" />
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:element>
      </xsd:sequence>
      <xsd:element minOccurs="0" name="CreationTimeStamp">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Time this optional service has been created.

-notes=
1. If the time zone is not identified the time is considered to be in the point of sale time zone.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:attribute name="TimeZone" type="xsd:string" use="optional">
            <xsd:annotation>
              <xsd:documentation source="DESCRIPTION" xml:lang="en">Time zone.
Example: UTC</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute ref="TimeStamp" use="required" />
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute ref="Source" use="optional" />
    <xsd:attribute ref="SourceRef" use="optional" />
  </xsd:complexType>
  <xsd:complexType name="ServiceRefInfoType">
    <xsd:annotation>
      <xsd:documentation source="DESCRIPTION" xml:lang="en">Service reference information.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element ref="TravelerIDRef" />
      <xsd:element minOccurs="0" maxOccurs="unbounded" ref="SegmentIDRef" />
      <xsd:element minOccurs="0" name="ServicePrice">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Service pricing information.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="CurrencyCode" type="CurrencyCodeType">
              <xsd:annotation>
                <xsd:documentation source="DESCRIPTION" xml:lang="en">Currency code.
Example: USD</xsd:documentation>
                <xsd:documentation source="ENCODING" xml:lang="en">For maximum interoperability, all currency codes are specified  in ISO 4217 format.
http://www.iso.org/iso/home/standards/iso8601.htm</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:choice minOccurs="0">
              <xsd:element minOccurs="0" name="Redemption" type="FullRedemptionType">
                <xsd:annotation>
                  <xsd:documentation source="DESCRIPTION" xml:lang="en">Award redemption unit and quantity information.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
              <xsd:element minOccurs="0" name="RedemptionCurrency" type="PartialRedemptionType">
                <xsd:annotation>
                  <xsd:documentation source="DESCRIPTION" xml:lang="en">PARTIAL redemption payment with conversion rate information.</xsd:documentation>
                  <xsd:documentation source="GUIDELINES" xml:lang="en">=notes=
1. Conversion rate is for the ServicePrice/CurrencyCode.</xsd:documentation>
                </xsd:annotation>
              </xsd:element>
            </xsd:choice>
          </xsd:sequence>
          <xsd:attribute name="Total" type="xsd:integer" use="required">
            <xsd:annotation>
              <xsd:documentation source="DESCRIPTION" xml:lang="en">Total amount.
Example: 7500</xsd:documentation>
              <xsd:documentation source="GUIDELINES" xml:lang="en">=notes=
1. Calculation: Total = base price +  total amount of taxes
2. This is the total amount for this service based on FQTV status, chosen price class and other optional service qualifiers.</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" ref="PriceRefKey" />
      <xsd:element name="Description" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Commercial name (title description) of offered service.
Example: 3rd Bag Fee</xsd:documentation>
          <xsd:documentation source="GUIDELINES" xml:lang="en">=notes=
1. UI tip: You may build a one line description to display by stringing the following values together:
    "Description"+"ServiceLocation"+"ServiceDate"+"DescriptionVariable" which could be displayed as shown here:
    "Airline Lounge Pass DFW 10May" where "Airline Lounge Pass" is the "Description", "DFW" the "ServiceLocation" and "10May" the "ServiceDate"
    "Premium Seat 9D" where "Premium Seat" is the "Description" and "9D" the "DescriptionVariable"</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="DescriptionVariable" type="SrvcDesVariableType">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Variable value for a selected service.
Examples: 
SEAT NUMBER: 9D
UID: GO8978 
PW: 1234</xsd:documentation>
          <xsd:documentation source="GUIDELINES" xml:lang="en">=notes=
1. Sample: for a Premium Seat this will be a seat number; for a annual pass of onboard WiFi this will be the credentials, etc.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="AssociationID" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation source="DESCRIPTION" xml:lang="en">Service association ID.
Example: S1</xsd:documentation>
        <xsd:documentation source="GUIDELINES" xml:lang="en">=notes=
1. This attribute is specified if a unique service association ID needs to be defined for this specific information.
2. For implementations that are NOT using element references for previously stored PNR data, the ID should start with a "S" and include an arbitrary value. For example: 
    S1 = Service 1
    S2 = Service 2</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute ref="ServiceType" use="required" />
    <xsd:attribute ref="ServiceCode" use="optional" />
    <xsd:attribute ref="SubCode" use="required" />
    <xsd:attribute ref="ReasonCode" use="required" />
    <xsd:attribute ref="Method" use="required" />
  </xsd:complexType>
  <xsd:complexType name="ServicesFilterType">
    <xsd:annotation>
      <xsd:documentation source="DESCRIPTION" xml:lang="en">Filtered list of offered services.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="Attributes">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Filtered list attributes.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="Group">
              <xsd:annotation>
                <xsd:documentation source="DESCRIPTION" xml:lang="en">Service group information.</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:attribute name="Code" type="xsd:string" use="required">
                  <xsd:annotation>
                    <xsd:documentation source="DESCRIPTION" xml:lang="en">Service group code.
Examples:
BD (BUNDLED SERVICE)
BG (BAGGAGE)
IE (IN-FLIGHT ENTERTAINMENT)
LG (LOUNGE)

For maximum interoperability, ATPCO Optional Services Industry Sub Codes encoding is recommended. Select a value from the enumerated list or enter a string value.</xsd:documentation>
                    <xsd:documentation source="GUIDELINES" xml:lang="en">=notes=
1. If using ATPCO-encoding, this is the Optional Services Group.</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
              </xsd:complexType>
            </xsd:element>
            <xsd:element minOccurs="0" maxOccurs="unbounded" name="SubGroup">
              <xsd:annotation>
                <xsd:documentation source="DESCRIPTION" xml:lang="en">Service sub-group(s).</xsd:documentation>
                <xsd:documentation source="GUIDELINES" xml:lang="en">ATPCO encoding samples:
Service Group BG: CT = Container, PP = Pre-paid excess baggage
Service Group GT: BU = Bus, LI = Limo
Service Group IE: IT = Internet access, TV = Television</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element name="Code" type="xsd:string">
                    <xsd:annotation>
                      <xsd:documentation source="DESCRIPTION" xml:lang="en">Service group subcode.

Examples:
Group BG: CT (CONTAINER), PP (PRE-PAID EXCESS BAGGAGE)
Group GT: BU (BUS), LI (LIMO)
Group IE: IT (INTERNET ACCESS), TV (TELEVISION)</xsd:documentation>
                      <xsd:documentation source="ENCODING" xml:lang="en">For maximum interoperability, it is recommended that ATPCO Optional Services Industry Sub Codes values are used. Use an ATPCO defined code or a string value.

= example values=
Group BG: CT = Container, PP = Pre-Paid Excess Baggage
Group GT: BU = Bus, LI = Limo
Group IE: IT = Internet Access; TV = Television</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="ServiceType">
    <xsd:annotation>
      <xsd:documentation source="DESCRIPTION" xml:lang="en">Optional service BUNDLE AND ITEM information.</xsd:documentation>
    </xsd:annotation>
    <xsd:attribute ref="Source" use="optional" />
    <xsd:attribute name="ApplyToAll" type="ChoiceType" use="optional">
      <xsd:annotation>
        <xsd:documentation source="DESCRIPTION" xml:lang="en">If Y, this offer, if selected, applies to all passengers in the PNR.
Example: Y (YES)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="Status" type="SrvcProcessStatusType" use="optional">
      <xsd:annotation>
        <xsd:documentation source="DESCRIPTION" xml:lang="en">Service processing/ settlement status.
Example: NN (REQUESTED)</xsd:documentation>
        <xsd:documentation source="ENCODING" xml:lang="en">Select a value from the enumerated list or enter a string value.
=enumerated values=
HK = CONFIRMED
NN = REQUESTED</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="SaleStatus" type="SrvcSalesStatusType" use="optional">
      <xsd:annotation>
        <xsd:documentation source="DESCRIPTION" xml:lang="en">Sale status.
Example: BOOKED</xsd:documentation>
        <xsd:documentation source="ENCODING" xml:lang="en">Select a value from the enumerated list or enter a string value.
=enumerated values=
BOOKED = SERVICES THAT HAVE BEEN SOLD/ BOOKED BUT DO NOT HAVE AN ISSUED EMD/DOCUMENT YET
ISSUED = SERVICES THAT HAVE BEEN SOLD/ BOOKED AND HAVE AN ISSUED EMD/DOCUMENT WITH VALID INFORMATION</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="ShoppingResponseType">
    <xsd:annotation>
      <xsd:documentation source="DESCRIPTION" xml:lang="en">Core shopping response information definition.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" ref="ShoppingResponseID" />
      <xsd:element minOccurs="0" ref="OfferTimeLimit" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="ShoppingResponseIDsType">
    <xsd:annotation>
      <xsd:documentation source="DESCRIPTION" xml:lang="en">Unique ID for a Collection of Offers from a Prior Shopping Response Message.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element ref="ShoppingResponseID">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Unique Offer Collection ID.
Example: 0981267543AQ</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="OfferIDs">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Unique Offer ID(s).</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:choice>
              <xsd:element ref="OfferID" />
              <xsd:element ref="ServiceID" />
            </xsd:choice>
            <xsd:element minOccurs="0" maxOccurs="unbounded" ref="TravelerIDRef" />
            <xsd:element minOccurs="0" maxOccurs="unbounded" ref="SegmentIDRef" />
          </xsd:sequence>
          <xsd:attribute name="OfferAction" use="optional">
            <xsd:annotation>
              <xsd:documentation source="DESCRIPTION" xml:lang="en">Offer action.
Examples:
INCLUDE
EXCLUDE
REPRICE</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction base="xsd:string">
                <xsd:enumeration value="EXCLUDE" />
                <xsd:enumeration value="INCLUDE" />
                <xsd:enumeration value="REPRICE" />
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:attribute>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="ShoppingResponseIDType">
    <xsd:annotation>
      <xsd:documentation source="DESCRIPTION" xml:lang="en">Unique Shopping Response ID.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent mixed="false">
      <xsd:extension base="OfferIDType" />
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="SocialNetworkingPaxType">
    <xsd:annotation>
      <xsd:documentation source="DESCRIPTION" xml:lang="en">Traveler social networking information.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="ServiceName" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Social network service name.
Examples:
FACEBOOK
TWITTER</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="UserName" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Traveler user or profile identification.
Example: JOHNSMITH2367</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="EmailAddress" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Associated email address.
Example: JSMITH@ANYWHERE.COM</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="TravelerIDRef">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Traveler association ID(s).
Examples:
1 or T1 = Traveler 1
2.1 or T2.1 = Traveler 2 with Infant on Lap

=notes=
1. This is one unique traveler reference and NO comma separated lists or ranges are supported.
2. Precede an infant ID number with a period (Example: T2.1, 2.1)
3. The location of the data set may be specified in the @Type attribute.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:complexContent mixed="false">
            <xsd:extension base="TravelerIDRefType">
              <xsd:attribute ref="PaxType" use="required" />
            </xsd:extension>
          </xsd:complexContent>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="SocialNetworkingType">
    <xsd:annotation>
      <xsd:documentation source="DESCRIPTION" xml:lang="en">Traveler social networking information.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="ServiceName" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Social network service name.
Examples:
FACEBOOK
TWITTER</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="UserName" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Traveler user or profile identification.
Example: JOHNSMITH2367</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="EmailAddress" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Associated email address.
Example: JSMITH@ANYWHERE.COM</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="SpecialSrvcOtherType">
    <xsd:annotation>
      <xsd:documentation source="DESCRIPTION" xml:lang="en">Special Traveler Needs Other information.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Code" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Special need code value.
Example: PET4</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="Description">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Special need code description.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="0" name="Text" type="FreeFormTextType">
              <xsd:annotation>
                <xsd:documentation source="DESCRIPTION" xml:lang="en">Description.
Example: PET IN CABIN</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
          <xsd:attribute name="DescCode" type="xsd:string" use="optional">
            <xsd:annotation>
              <xsd:documentation source="DESCRIPTION" xml:lang="en">Description context.
Examples:
WEIGHT
NUMBER
PEDICURE</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="SpecialSrvcPaxType">
    <xsd:annotation>
      <xsd:documentation source="DESCRIPTION" xml:lang="en">Special Traveler Needs with associated traveler.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="SSRType">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">IATA SSR-encoded special needs passenger attributes.</xsd:documentation>
          <xsd:documentation source="GUIDELINES" xml:lang="en">=notes=
1. A four-letter IATA Special Service Request (SSR) codes to be put in a passenger's PNR field, as stated in the IATA book, edition 5.12</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:complexContent mixed="false">
            <xsd:extension base="SpecialSrvcSSRType">
              <xsd:sequence>
                <xsd:element minOccurs="0" maxOccurs="unbounded" name="TravelerIDRef">
                  <xsd:annotation>
                    <xsd:documentation source="DESCRIPTION" xml:lang="en">Traveler association ID(s).
Examples:
1 or T1 = Traveler 1
2.1 or T2.1 = Traveler 2 with Infant on Lap

=notes=
1. This is one unique traveler reference and NO comma separated lists or ranges are supported.
2. Precede an infant ID number with a period (Example: T2.1, 2.1)
3. The location of the data set may be specified in the @Type attribute.</xsd:documentation>
                  </xsd:annotation>
                  <xsd:complexType>
                    <xsd:complexContent mixed="false">
                      <xsd:extension base="TravelerIDRefType">
                        <xsd:attribute ref="PaxType" use="required" />
                      </xsd:extension>
                    </xsd:complexContent>
                  </xsd:complexType>
                </xsd:element>
              </xsd:sequence>
            </xsd:extension>
          </xsd:complexContent>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="OtherType">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Free flow text or arbitrary-encoded special needs passenger attributes.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:complexContent mixed="false">
            <xsd:extension base="SpecialSrvcOtherType">
              <xsd:sequence>
                <xsd:element minOccurs="0" maxOccurs="unbounded" name="TravelerIDRef">
                  <xsd:annotation>
                    <xsd:documentation source="DESCRIPTION" xml:lang="en">Traveler association ID(s).
Examples:
1 or T1 = Traveler 1
2.1 or T2.1 = Traveler 2 with Infant on Lap

=notes=
1. This is one unique traveler reference and NO comma separated lists or ranges are supported.
2. Precede an infant ID number with a period (Example: T2.1, 2.1)
3. The location of the data set may be specified in the @Type attribute.</xsd:documentation>
                  </xsd:annotation>
                  <xsd:complexType>
                    <xsd:complexContent mixed="false">
                      <xsd:extension base="TravelerIDRefType">
                        <xsd:attribute ref="PaxType" use="required" />
                      </xsd:extension>
                    </xsd:complexContent>
                  </xsd:complexType>
                </xsd:element>
              </xsd:sequence>
            </xsd:extension>
          </xsd:complexContent>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="SpecialSrvcSegType">
    <xsd:annotation>
      <xsd:documentation source="DESCRIPTION" xml:lang="en">Special Traveler Needs with associated flight segment.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="unbounded" ref="SegmentIDRef" />
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="SSRType">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">IATA SSR-encoded special needs passenger attributes.</xsd:documentation>
          <xsd:documentation source="GUIDELINES" xml:lang="en">=notes=
1. A four-letter IATA Special Service Request (SSR) codes to be put in a passenger's PNR field, as stated in the IATA book, edition 5.12</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:complexContent mixed="false">
            <xsd:extension base="SpecialSrvcSSRType" />
          </xsd:complexContent>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="OtherType">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Free flow text or arbitrary-encoded special needs passenger attributes.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:complexContent mixed="false">
            <xsd:extension base="SpecialSrvcOtherType" />
          </xsd:complexContent>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="SpecialSrvcSSRType">
    <xsd:annotation>
      <xsd:documentation source="DESCRIPTION" xml:lang="en">Special Traveler Needs SSR information.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Code" type="SSRCodeType">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Passenger assistance SSR code value.
Examples:
EXST (EXTRA SEAT REQUIRED)
INFT (INFANT ACCOMPANYING ADULT PASSENGER)
PETC (PASSENGER TRAVELING WITH PET IN CABIN)
UMNR (UNACCOMPANIED MINOR)

=notes=
1. Although optional in the schema, some SSR codes may require associated description elements.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Description">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">IATA SSR code description.

=notes=
1. Some SSR codes REQUIRE description elements.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="0" name="Text" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation source="DESCRIPTION" xml:lang="en">Description.
Example: UNACCOMPANIED MINOR</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
          <xsd:attribute name="DescCode" type="xsd:string" use="optional">
            <xsd:annotation>
              <xsd:documentation source="DESCRIPTION" xml:lang="en">Description context.
Examples:
WEIGHT
NUMBER
PEDICURE</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="SpecialSrvcType">
    <xsd:annotation>
      <xsd:documentation source="DESCRIPTION" xml:lang="en">Special Traveler Needs information.

=notes=
1. Special Traveler Needs may OPTIONALLY be associated to one or more defined travelers.
2. Special Traveler Needs apply to ALL travelers if no traveler associations are specified.
3. Both IATA  (four character) Special Service Request (SSR) Codes and implementer-proprietary encoding is supported.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="SSRType">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">IATA SSR-encoded special needs passenger attributes.</xsd:documentation>
          <xsd:documentation source="GUIDELINES" xml:lang="en">=notes=
1. A four-letter IATA Special Service Request (SSR) codes to be put in a passenger's PNR field, as stated in the IATA book, edition 5.12</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:complexContent mixed="false">
            <xsd:extension base="SpecialSrvcSSRType">
              <xsd:sequence>
                <xsd:element minOccurs="0" maxOccurs="unbounded" ref="TravelerIDRef" />
              </xsd:sequence>
            </xsd:extension>
          </xsd:complexContent>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="OtherType">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Free flow text or arbitrary-encoded special needs passenger attributes.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:complexContent mixed="false">
            <xsd:extension base="SpecialSrvcOtherType">
              <xsd:sequence>
                <xsd:element minOccurs="0" maxOccurs="unbounded" ref="TravelerIDRef" />
              </xsd:sequence>
            </xsd:extension>
          </xsd:complexContent>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="SrvcDesVariableType">
    <xsd:annotation>
      <xsd:documentation source="DESCRIPTION" xml:lang="en">Additional description value for selected service.
Examples:
SEAT: 9D
DISCOUNT: 15P</xsd:documentation>
      <xsd:documentation source="GUIDELINES" xml:lang="en">=notes=
1. Usage examples:
    Premium Seat: this will be a seat number
    Annual pass of onboard WiFi: this will be the credentials</xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="xsd:string" />
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="SrvcProcessInfoType">
    <xsd:annotation>
      <xsd:documentation source="DESCRIPTION" xml:lang="en">Service transaction processing information.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element maxOccurs="unbounded" name="Text">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Information text.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:minLength value="1" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute ref="Source" use="optional" />
    <xsd:attribute ref="SourceRef" use="optional" />
  </xsd:complexType>
  <xsd:complexType name="SurchargeBasicType">
    <xsd:annotation>
      <xsd:documentation source="DESCRIPTION" xml:lang="en">Basic surcharge information.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Designator">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Surcharge designator.
Example: US</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:minLength value="1" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element minOccurs="0" name="Description">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Surcharge description.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:minLength value="1" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element minOccurs="0" name="FiledCurrencyCode" type="CurrencyCodeType">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Fare component pricing currency code.
Example: HKD

For maximum interoperability, all currency designators are specified in ISO (three character) 4217 format.</xsd:documentation>
          <xsd:documentation source="GUIDELINES" xml:lang="en">=notes=
1. This should be specified if the fare component pricing currency code is DIFFERENT from the POS currency code.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="FiledAmount" type="xsd:integer">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Filed fare amount.
Example: 6400</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Amount" type="xsd:integer" use="required">
      <xsd:annotation>
        <xsd:documentation source="DESCRIPTION" xml:lang="en">Individual surcharge amount.
Example: 270</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="SurchargesType">
    <xsd:annotation>
      <xsd:documentation source="DESCRIPTION" xml:lang="en">Surcharge information.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="Surcharge" type="SurchargeType">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Surcharge break down information.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Amount" type="xsd:integer" use="required">
      <xsd:annotation>
        <xsd:documentation source="DESCRIPTION" xml:lang="en">Total surcharges amount.
Example: 2000</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="SurchargeType">
    <xsd:annotation>
      <xsd:documentation source="DESCRIPTION" xml:lang="en">Surcharge detail.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent mixed="false">
      <xsd:extension base="SurchargeBasicType">
        <xsd:attribute name="Segment" type="xsd:integer" use="optional">
          <xsd:annotation>
            <xsd:documentation source="DESCRIPTION" xml:lang="en">Segment number to which the surcharge applies.
Example: 1</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute ref="ONPoint" use="optional" />
        <xsd:attribute ref="OFFPoint" use="optional" />
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="SystemSourceType">
    <xsd:annotation>
      <xsd:documentation source="DESCRIPTION" xml:lang="en">System source detail.
=notes=
1. This information is used for auditing and traceability.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="IPAddress" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">IP address of the requestor system.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="IPPort" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">IP port associated with specified @IPAddress.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="TaxCollectionType">
    <xsd:annotation>
      <xsd:documentation source="DESCRIPTION" xml:lang="en">Tax breakdown information.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Designator" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Tax or surcharge designator.
Example: US</xsd:documentation>
          <xsd:documentation source="ENCODING" xml:lang="en">ATPCO ENCODING: In ATPCO tax product, the Designator element maps to the ATPCO Tax Code element.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Nature" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Tax nature.
Example: US2</xsd:documentation>
          <xsd:documentation source="ENCODING" xml:lang="en">ATPCO ENCODING: In ATPCO tax product, the Nature element maps to the IATA/ATPCO ISO Nation Code.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Description" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Tax or surcharge description.
Example: US National Transportation</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="CollectionPoint">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Tax data break down by collection point for XF (extra fee) and ZP (federal tax segment) taxes.</xsd:documentation>
          <xsd:documentation source="GUIDELINES" xml:lang="en">=notes=
1. The sum of individual amounts entered here must be equal to the Amount entered in the Tax element.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="CurrencyCode" type="CurrencyCodeType">
              <xsd:annotation>
                <xsd:documentation source="DESCRIPTION" xml:lang="en">Currency encoding.
Example: USD</xsd:documentation>
                <xsd:documentation source="ENCODING" xml:lang="en">For maximum interoperability, all currency codes are specified  in ISO 4217 format.
http://www.iso.org/iso/home/standards/iso8601.htm</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="AirportAmount" type="xsd:positiveInteger">
              <xsd:annotation>
                <xsd:documentation source="DESCRIPTION" xml:lang="en">Amount filed by the airport in the currency applicable to the geographical location of the airport.
Example: 2789</xsd:documentation>
                <xsd:documentation source="GUIDELINES" xml:lang="en">=notes=
1. This is the same as shown in FareCalculation line.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="AirportCode" type="AirportCityCodeType">
              <xsd:annotation>
                <xsd:documentation source="DESCRIPTION" xml:lang="en">Segment departure or arrival airport code.
Example: CDG</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
          <xsd:attribute name="Amount" type="xsd:integer" use="optional">
            <xsd:annotation>
              <xsd:documentation source="DESCRIPTION" xml:lang="en">Tax amount for collection point.
Example: 2500</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Amount" type="xsd:integer" use="required">
      <xsd:annotation>
        <xsd:documentation source="DESCRIPTION" xml:lang="en">Individual tax amount.
Example: 2500</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="Y" name="Refundable" type="ChoiceType" use="optional">
      <xsd:annotation>
        <xsd:documentation source="DESCRIPTION" xml:lang="en">If Y, tax is refundable.
Example: Y (YES)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="Y" name="MustCollect" type="ChoiceType" use="optional">
      <xsd:annotation>
        <xsd:documentation source="DESCRIPTION" xml:lang="en">If Y, tax must be collected regardless of monetary or redemption settlement form, including free services.
Example: Y (YES)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="TaxesCollectionType">
    <xsd:annotation>
      <xsd:documentation source="DESCRIPTION" xml:lang="en">Taxes information.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element maxOccurs="unbounded" name="Tax" type="TaxCollectionType">
        <xsd:annotation>
          <xsd:documentation>Tax break down information.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Amount" type="xsd:integer" use="required">
      <xsd:annotation>
        <xsd:documentation source="DESCRIPTION" xml:lang="en">Total taxes amount.
Example: 750</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="TaxesType">
    <xsd:annotation>
      <xsd:documentation source="DESCRIPTION" xml:lang="en">Taxes information.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element maxOccurs="unbounded" name="Tax" type="TaxType">
        <xsd:annotation>
          <xsd:documentation>Tax break down information.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Amount" type="xsd:integer" use="required">
      <xsd:annotation>
        <xsd:documentation source="DESCRIPTION" xml:lang="en">Total taxes amount.
Example: 750</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="TaxType">
    <xsd:annotation>
      <xsd:documentation source="DESCRIPTION" xml:lang="en">Tax breakdown information.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Designator" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Tax or surcharge designator.
Example: US</xsd:documentation>
          <xsd:documentation source="ENCODING" xml:lang="en">ATPCO ENCODING: In ATPCO tax product, the Designator element maps to the ATPCO Tax Code element.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Nature" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Tax nature.
Example: US2</xsd:documentation>
          <xsd:documentation source="ENCODING" xml:lang="en">ATPCO ENCODING: In ATPCO tax product, the Nature element maps to the IATA/ATPCO ISO Nation Code.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Description" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Tax or surcharge description.
Example: US National Transportation</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Amount" type="xsd:integer" use="required">
      <xsd:annotation>
        <xsd:documentation source="DESCRIPTION" xml:lang="en">Individual tax amount.
Example: 2500</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="Y" name="Refundable" type="ChoiceType" use="optional">
      <xsd:annotation>
        <xsd:documentation source="DESCRIPTION" xml:lang="en">If Y, tax is refundable.
Example: Y (YES)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute default="Y" name="MustCollect" type="ChoiceType" use="optional">
      <xsd:annotation>
        <xsd:documentation source="DESCRIPTION" xml:lang="en">If Y, tax must be collected regardless of monetary or redemption settlement form, including free services.
Example: Y (YES)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="TerminalType">
    <xsd:annotation>
      <xsd:documentation source="DESCRIPTION" xml:lang="en">Airport terminal.
Example: 2F</xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="xsd:string" />
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="TicketDesignatorType">
    <xsd:annotation>
      <xsd:documentation source="DESCRIPTION" xml:lang="en">Ticket designator.
Example: AM10AC</xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="xsd:string" />
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="TicketingInfoType">
    <xsd:annotation>
      <xsd:documentation source="DESCRIPTION" xml:lang="en">Ticket Issuance information.</xsd:documentation>
    </xsd:annotation>
    <xsd:all>
      <xsd:element minOccurs="0" name="CityCode" type="AirportCityCodeType">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">City code.
Example: NYC</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="CountryCode" type="CountryCodeType">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Country code.
Example: US</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Date" type="DateType">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Future ticketing date.
Example: 2013-01-13</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:all>
  </xsd:complexType>
  <xsd:complexType name="TimeZoneType">
    <xsd:annotation>
      <xsd:documentation source="DESCRIPTION" xml:lang="en">Time zone abbreviation.
Example: ET</xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:annotation>
        <xsd:documentation source="DESCRIPTION">The time zone name or abbreviation.</xsd:documentation>
      </xsd:annotation>
      <xsd:extension base="xsd:string">
        <xsd:attribute name="UTCOffset" type="xsd:string" use="optional">
          <xsd:annotation>
            <xsd:documentation source="DESCRIPTION" xml:lang="en">The Coordinated Universal Time (UTC) time zone offset.
Example: - 5h</xsd:documentation>
            <xsd:documentation source="GUIDELINES" xml:lang="en">=notes=
1. A UTC offset is typically represented with a plus or minus sign, an hour increment and an hour designator, such as - 5h.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="DaylightSavings" type="ChoiceType" use="optional">
          <xsd:annotation>
            <xsd:documentation source="DESCRIPTION" xml:lang="en">If Y, daylight (or summer) savings time is used in the time zone.
Example: N (NO)</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="TktInExchangeForType">
    <xsd:annotation>
      <xsd:documentation source="DESCRIPTION" xml:lang="en">Auto Exchange application information.</xsd:documentation>
      <xsd:documentation source="GUIDELINES" xml:lang="en">=notes=
1. IMPORTANT: This is MANDATORY information for AutoExchange applications, such as ReShop from ITA.
2. If used, the attribute @AutoExchange in the PricingInfo node MUST also be set to Y.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element maxOccurs="unbounded" name="TicketNumber">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Ticket number.
Example: 0110987654321</xsd:documentation>
          <xsd:documentation source="GUIDELINES" xml:lang="en">=notes=
1. This is the ticket number to be used for the Auto Exchange proccess.
2. In the case of a conjunctive set, this is the 1st number of the set.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:simpleContent>
            <xsd:extension base="xsd:integer">
              <xsd:attribute ref="Source" use="required" />
              <xsd:attribute name="TravelerID" type="xsd:string" use="required">
                <xsd:annotation>
                  <xsd:documentation source="DESCRIPTION" xml:lang="en">Unique association ID.
Examples:
1 or T1 = Traveler 1
2 or T2 = Traveler 2
2.1 or T2.1 = Traveler 2 with Infant on Lap

IMPORTANT: See encoding annotation before setting this value.</xsd:documentation>
                  <xsd:documentation source="ENCODING" xml:lang="en">=notes=
1. This attribute is specified if a unique traveler association ID needs to be defined for this specific information, such as for associating this traveler to optional services.
2. For implementations that are NOT using element references for previously stored PNR data, the ID should start with a "T" and include an arbitrary value. For example: 
    T1 = Traveler 1
    T2 = Traveler 2
    T2.1 = Traveler 2 with Infant on Lap
3.  For implementations that ARE using element references for previously stored data (e.g. PNR data), the ID should be the numeric TravelerElementNumber of the booked PNR. For example: 
    1 = Traveler 1
    2 = Traveler 2
    2.1 = Traveler 2 with Infant on Lap</xsd:documentation>
                </xsd:annotation>
              </xsd:attribute>
              <xsd:attribute ref="PaxType" use="required" />
            </xsd:extension>
          </xsd:simpleContent>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="TouchPointType">
    <xsd:annotation>
      <xsd:documentation source="DESCRIPTION" xml:lang="en">Message Touchpoint information.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Device" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Touchpoint device type.
Example: MOD (MOBILE DEVICE)

For maximum interoperability, Refer to IATA PADIS code list TDT - Touchpoint Device Type. Select a code value or enter a value that is bilaterally agreed upon between trading partners.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Transaction" type="TPointTransCatType">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Touchpoint transaction category.
Example: CHE (CHECK-IN)

For maximum interoperability, Refer to IATA PADIS code list TTT - Touchpoint Transaction Type. Select a code value or enter a value that is bilaterally agreed upon between trading partners.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="TravelerAgeType">
    <xsd:annotation>
      <xsd:documentation source="DESCRIPTION" xml:lang="en">Traveler age.
Example: 01</xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="xsd:string" />
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="TravelerIDRefType">
    <xsd:annotation>
      <xsd:documentation source="DESCRIPTION" xml:lang="en">Traveler association ID(s).
Examples:
1 or T1 = Traveler 1
2.1 or T2.1 = Traveler 2 with Infant on Lap

=notes=
1. This is one unique traveler reference and NO comma separated lists or ranges are supported.
2. Precede an infant ID number with a period (Example: T2.1, 2.1)
3. The location of the data set may be specified in the @Type attribute.</xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="xsd:string">
        <xsd:attribute ref="RefType" use="optional" />
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="TravelerInfantType">
    <xsd:annotation>
      <xsd:documentation source="DESCRIPTION" xml:lang="en">Infant on lap traveler information.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Surname" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Infant traveler family (last or sur) name.
Example: SMITH</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="GivenName" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Infant traveler given (first) name.
Example: WILLIAM</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="DateOfBirth" type="TravelerDateOfBirthType">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Date of birth.
Example: 2013-01-13</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Gender" type="TravelerGenderType">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Infant gender.
Example: FI (FEMALE INFANT)</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute default="INF" name="Type" type="PAXType" use="optional">
      <xsd:annotation>
        <xsd:documentation source="DESCRIPTION" xml:lang="en">LAP INFANT passenger type.
Example: JNF(CONTRACT BULK INFANT WITHOUT A SEAT)</xsd:documentation>
        <xsd:documentation source="GUIDELINES" xml:lang="en">=notes=
1. This attribute has a default of INF (Infant on lap) as the infant passenger type, but supports alternate encoding, such as JNF (Contract Bulk Infant without a seat).</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="AssociationID" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation source="DESCRIPTION" xml:lang="en">Unique association ID.
Examples:
1 or T1 = Traveler 1
2 or T2 = Traveler 2
2.1 or T2.1 = Traveler 2 with Infant on Lap</xsd:documentation>
        <xsd:documentation source="ENCODING" xml:lang="en">=notes=
1. This attribute is specified if a unique traveler association ID needs to be defined for this specific information, such as for associating this traveler to optional services.
2. For implementations that are NOT using element references for previously stored PNR data, the ID should start with a "T" and include an arbitrary value. For example: 
    T1 = Traveler 1
    T2 = Traveler 2
    T2.1 = Traveler 2 with Infant on Lap
3.  For implementations that ARE using element references for previously stored data (e.g. PNR data), the ID should be the numeric TravelerElementNumber of the booked PNR. For example: 
    1 = Traveler 1
    2 = Traveler 2
    2.1 = Traveler 2 with Infant on Lap</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="TravelerInfoType">
    <xsd:annotation>
      <xsd:documentation source="DESCRIPTION" xml:lang="en">Number of passengers of the specified type.
Example: 2</xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="xsd:integer">
        <xsd:attribute name="Type" type="PAXType" use="required">
          <xsd:annotation>
            <xsd:documentation source="DESCRIPTION" xml:lang="en">Passenger type for pricing.
Example: CHD</xsd:documentation>
            <xsd:documentation source="ENCODING" xml:lang="en">For maximum interoperability, it is recommended that IATA three character passenger type codes be used:
ADT= Adult
CHD = Child
INF = Infant
GOV = Government
MIL = Military
SRC = Senior citizen
STD = Student
YTH = Youth</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="TravelerNameType">
    <xsd:annotation>
      <xsd:documentation source="DESCRIPTION" xml:lang="en">Traveler Name information.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Surname" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Traveler family (last or sur) name.
Example: SMITH</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="GivenName" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Traveler family (first or given) name.
Example: JOHN</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Initial" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Traveler middle initial or name.
Examples: G, GEORGE </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Title" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Traveler title.
Example: MR</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:choice minOccurs="0">
        <xsd:element minOccurs="0" ref="Age" />
        <xsd:element minOccurs="0" ref="DateOfBirth" />
      </xsd:choice>
      <xsd:element minOccurs="0" name="NameRemark" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Name remark.
Example: A123456</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" ref="Gender" />
      <xsd:element minOccurs="0" ref="ProfileIndex" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="TravelerRefInfoType">
    <xsd:annotation>
      <xsd:documentation source="DESCRIPTION" xml:lang="en">Traveler information.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="Surname" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Traveler family (last or sur) name.
Example: SMITH</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="GivenName" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Traveler family (first or given) name.
Example: JOHN</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Initial" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Traveler middle initial or name.
Examples: G, GEORGE </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Title" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Traveler title.
Example: MR</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:choice minOccurs="0">
        <xsd:element minOccurs="0" ref="Age" />
        <xsd:element minOccurs="0" ref="DateOfBirth" />
      </xsd:choice>
      <xsd:element minOccurs="0" ref="Gender" />
      <xsd:element minOccurs="0" ref="InfantData" />
      <xsd:element minOccurs="0" ref="ProfileIndex" />
    </xsd:sequence>
    <xsd:attribute name="AssociationID" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation source="DESCRIPTION" xml:lang="en">Unique association ID.
Examples:
1 or T1 = Traveler 1
2.1 or T2.1 = Traveler 2 with Infant on Lap</xsd:documentation>
        <xsd:documentation source="ENCODING" xml:lang="en">=notes=
1. This attribute is specified if a unique traveler association ID needs to be defined for this specific information, such as for associating this traveler to optional services.
2. For implementations that are NOT using element references for previously stored PNR data, the ID should start with a "T" and include an arbitrary value. For example: 
    T1 = Traveler 1
    T2 = Traveler 2
    T2.1 = Traveler 2 with Infant on Lap
3.  For implementations that ARE using element references for previously stored data (e.g. PNR data), the ID should be the numeric TravelerElementNumber of the booked PNR. For example: 
    1 = Traveler 1
    2 = Traveler 2
    2.1 = Traveler 2 with Infant on Lap</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="PaxType" type="PAXType" use="required">
      <xsd:annotation>
        <xsd:documentation source="DESCRIPTION" xml:lang="en">Passenger type code.
Example: ADT (ADULT)</xsd:documentation>
        <xsd:documentation source="WARNINGS" xml:lang="en">=warnings=
Check With Your Trading Partners: Implemented (accepted) IATA passenger type codes may vary between trading partners.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="TravelerScore">
    <xsd:annotation>
      <xsd:documentation source="DESCRIPTION" xml:lang="en">Traveler valuation score.
Example: 5</xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="xsd:string" />
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="USDOTType">
    <xsd:annotation>
      <xsd:documentation source="DESCRIPTION" xml:lang="en">US Department of Transportation (DOT) Reservation or Disclosure baggage rule.
Example: D ( BAGGAGE IS SUBJECT DISCLOSURE BUT NOT US DOT RESERVATION)</xsd:documentation>
      <xsd:documentation source="ENCODING" xml:lang="en">Select a value from the enumerated list or enter a string value.
=enumerated values=
N = NOT SUBJECT TO US DOT RESERVATION OR DISCLOSURE
Y = SUBJECT TO US DOT RESERVATION AND DISCLOSURE
D = SUBJECT TO DISCLOSURE BUT NOT TO US DOT RESERVATION</xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="DisclosureCodeType">
        <xsd:attribute name="Disclosure" type="xsd:string" use="optional">
          <xsd:annotation>
            <xsd:documentation source="DESCRIPTION" xml:lang="en">Disclosure description.</xsd:documentation>
            <xsd:documentation source="GUIDELINES" xml:lang="en">=notes=
1. Elaborates on the US DOT tag to provide a message specifying whether disclosure and/or US DOT reservations apply.</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="Info" type="xsd:string" use="optional">
          <xsd:annotation>
            <xsd:documentation source="DESCRIPTION" xml:lang="en">Additional information.</xsd:documentation>
            <xsd:documentation source="GUIDELINES" xml:lang="en">=notes=
1. Provides additional information as to why the US DOT reservations do or do not apply (e.g. International travel to/from US).</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="UserInputOtherType">
    <xsd:annotation>
      <xsd:documentation source="DESCRIPTION" xml:lang="en">User input required for a service.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="UserText">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">User entry information.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:attribute name="InputID" type="xsd:string" use="required">
            <xsd:annotation>
              <xsd:documentation source="DESCRIPTION" xml:lang="en">Associated backend component.
Examples:
PUTime
DOTime
Address</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="UserInstruction" type="xsd:string" use="optional">
            <xsd:annotation>
              <xsd:documentation source="DESCRIPTION" xml:lang="en">User instructions for required input.

=notes=
1. A client application should use this as a label for the user input box.</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Required" type="ChoiceType" use="optional">
            <xsd:annotation>
              <xsd:documentation source="DESCRIPTION" xml:lang="en">If Y, user input is mandatory.
Example: Y (YES)</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="OptionList" type="OptionListType">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">User selection based on a dropdown menu.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="UserChoices">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">User choice(s) information.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="FlightOptions">
              <xsd:annotation>
                <xsd:documentation source="DESCRIPTION" xml:lang="en">Alternate flights offered with this service, such as stand by flight choices (or reaccommodation flights).</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element maxOccurs="unbounded" name="Flight">
                    <xsd:annotation>
                      <xsd:documentation source="DESCRIPTION" xml:lang="en">Flight information.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                      <xsd:sequence>
                        <xsd:element name="ElementNumber">
                          <xsd:annotation>
                            <xsd:documentation>PNR element number.
Example: 1</xsd:documentation>
                          </xsd:annotation>
                          <xsd:complexType>
                            <xsd:simpleContent>
                              <xsd:extension base="xsd:string">
                                <xsd:attribute name="TattooNumber" type="xsd:string" use="optional">
                                  <xsd:annotation>
                                    <xsd:documentation source="DESCRIPTION" xml:lang="en">Tattoo number.
Example: 2</xsd:documentation>
                                  </xsd:annotation>
                                </xsd:attribute>
                                <xsd:attribute name="TattooQualifier" type="xsd:string" use="optional">
                                  <xsd:annotation>
                                    <xsd:documentation source="DESCRIPTION" xml:lang="en">Tattoo qualifier.
Example: ST</xsd:documentation>
                                  </xsd:annotation>
                                </xsd:attribute>
                              </xsd:extension>
                            </xsd:simpleContent>
                          </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="Departure">
                          <xsd:annotation>
                            <xsd:documentation source="DESCRIPTION" xml:lang="en">Departure information.</xsd:documentation>
                          </xsd:annotation>
                          <xsd:complexType>
                            <xsd:sequence>
                              <xsd:element ref="AirportCode" />
                              <xsd:element name="AirportName" type="xsd:string">
                                <xsd:annotation>
                                  <xsd:documentation source="DESCRIPTION" xml:lang="en">Airport name.
Example: LAGUARDIA AIRPORT</xsd:documentation>
                                </xsd:annotation>
                              </xsd:element>
                              <xsd:element name="Date" type="DateType">
                                <xsd:annotation>
                                  <xsd:documentation source="DESCRIPTION" xml:lang="en">Departure date.
Example: 2013-01-11</xsd:documentation>
                                </xsd:annotation>
                              </xsd:element>
                              <xsd:element name="Time" type="TimeType">
                                <xsd:annotation>
                                  <xsd:documentation source="DESCRIPTION" xml:lang="en">Departure time.
Example: 11:32</xsd:documentation>
                                </xsd:annotation>
                              </xsd:element>
                              <xsd:element minOccurs="0" name="Terminal" type="TerminalType">
                                <xsd:annotation>
                                  <xsd:documentation source="DESCRIPTION" xml:lang="en">Airport terminal.
Example: 2F</xsd:documentation>
                                </xsd:annotation>
                              </xsd:element>
                            </xsd:sequence>
                          </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="Arrival">
                          <xsd:annotation>
                            <xsd:documentation source="DESCRIPTION" xml:lang="en">Arrival information.</xsd:documentation>
                          </xsd:annotation>
                          <xsd:complexType>
                            <xsd:sequence>
                              <xsd:element ref="AirportCode" />
                              <xsd:element name="AirportName" type="xsd:string">
                                <xsd:annotation>
                                  <xsd:documentation source="DESCRIPTION" xml:lang="en">Airport name.
Example: CHARLES DE GAULLE AIRPORT</xsd:documentation>
                                </xsd:annotation>
                              </xsd:element>
                              <xsd:element name="Date" type="DateType">
                                <xsd:annotation>
                                  <xsd:documentation source="DESCRIPTION" xml:lang="en">Arrival date.
Example: 2013-01-15</xsd:documentation>
                                </xsd:annotation>
                              </xsd:element>
                              <xsd:element name="Time" type="TimeType">
                                <xsd:annotation>
                                  <xsd:documentation source="DESCRIPTION" xml:lang="en">Arrival time.
Example: 14:30</xsd:documentation>
                                </xsd:annotation>
                              </xsd:element>
                              <xsd:element minOccurs="0" name="ChangeOfDay" type="xsd:integer">
                                <xsd:annotation>
                                  <xsd:documentation source="DESCRIPTION" xml:lang="en">Number of days between arrival and departure.
Example: 1</xsd:documentation>
                                </xsd:annotation>
                              </xsd:element>
                              <xsd:element minOccurs="0" name="Terminal" type="TerminalType">
                                <xsd:annotation>
                                  <xsd:documentation>Terminal.
Example: 2F</xsd:documentation>
                                </xsd:annotation>
                              </xsd:element>
                            </xsd:sequence>
                          </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="Carrier">
                          <xsd:annotation>
                            <xsd:documentation source="DESCRIPTION" xml:lang="en">Carrier information.</xsd:documentation>
                          </xsd:annotation>
                          <xsd:complexType>
                            <xsd:sequence>
                              <xsd:element ref="AirlineCode" />
                              <xsd:element name="AirlineName" type="xsd:string">
                                <xsd:annotation>
                                  <xsd:documentation source="DESCRIPTION" xml:lang="en">Airline name.
Example: UNITED AIRLINES</xsd:documentation>
                                </xsd:annotation>
                              </xsd:element>
                              <xsd:element minOccurs="0" ref="FlightNumber" />
                            </xsd:sequence>
                          </xsd:complexType>
                        </xsd:element>
                        <xsd:element minOccurs="0" name="OperatingCarrier">
                          <xsd:annotation>
                            <xsd:documentation source="DESCRIPTION" xml:lang="en">Operating carrier information.</xsd:documentation>
                          </xsd:annotation>
                          <xsd:complexType>
                            <xsd:sequence>
                              <xsd:element minOccurs="0" ref="AirlineCode" />
                              <xsd:element name="AirlineName" type="xsd:string">
                                <xsd:annotation>
                                  <xsd:documentation source="DESCRIPTION" xml:lang="en">Airline name.
Example: UNITED AIRLINES</xsd:documentation>
                                </xsd:annotation>
                              </xsd:element>
                              <xsd:element minOccurs="0" ref="FlightNumber" />
                              <xsd:element minOccurs="0" ref="ClassOfService" />
                              <xsd:element minOccurs="0" name="Comment" type="FreeFormTextType">
                                <xsd:annotation>
                                  <xsd:documentation source="DESCRIPTION" xml:lang="en">Free form text additional flight information.</xsd:documentation>
                                </xsd:annotation>
                              </xsd:element>
                            </xsd:sequence>
                          </xsd:complexType>
                        </xsd:element>
                        <xsd:element minOccurs="0" name="Equipment" type="EquipmentIDType">
                          <xsd:annotation>
                            <xsd:documentation source="DESCRIPTION" xml:lang="en">Aircraft equipment information.</xsd:documentation>
                          </xsd:annotation>
                        </xsd:element>
                        <xsd:element minOccurs="0" ref="NumberOfStops" />
                        <xsd:element minOccurs="0" ref="FlightDuration" />
                        <xsd:element minOccurs="0" ref="Meals" />
                        <xsd:element minOccurs="0" ref="Frequency" />
                        <xsd:element minOccurs="0" name="ElectronicTicketing" type="ChoiceType">
                          <xsd:annotation>
                            <xsd:documentation source="DESCRIPTION" xml:lang="en">If Y, an electronic ticket may be issued.
Example: Y (YES)</xsd:documentation>
                          </xsd:annotation>
                        </xsd:element>
                        <xsd:element ref="ClassOfService" />
                        <xsd:element name="ActionCode">
                          <xsd:annotation>
                            <xsd:documentation source="DESCRIPTION" xml:lang="en">Reservation system action code value.</xsd:documentation>
                          </xsd:annotation>
                          <xsd:complexType>
                            <xsd:simpleContent>
                              <xsd:extension base="xsd:string">
                                <xsd:attribute name="Status" use="optional">
                                  <xsd:annotation>
                                    <xsd:documentation source="DESCRIPTION" xml:lang="en">Confirmed sell HK code.
Examples:
K (CONFIRMED)
U (UNKNOWN)</xsd:documentation>
                                    <xsd:documentation source="ENCODING" xml:lang="en">Select a value from the enumerated list or enter a string value.
=enumerated values=
K = CONFIRMED
U = UNKNOWN</xsd:documentation>
                                    <xsd:documentation source="GUIDELINES" xml:lang="en">=notes=
1. This is the HK code if the sell is confirmed (sold in carrier system).</xsd:documentation>
                                  </xsd:annotation>
                                  <xsd:simpleType>
                                    <xsd:restriction base="xsd:string">
                                      <xsd:length value="1" />
                                      <xsd:enumeration value="K">
                                        <xsd:annotation>
                                          <xsd:documentation>CONFIRMED</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:enumeration>
                                      <xsd:enumeration value="U">
                                        <xsd:annotation>
                                          <xsd:documentation>UNKNOWN</xsd:documentation>
                                        </xsd:annotation>
                                      </xsd:enumeration>
                                    </xsd:restriction>
                                  </xsd:simpleType>
                                </xsd:attribute>
                              </xsd:extension>
                            </xsd:simpleContent>
                          </xsd:complexType>
                        </xsd:element>
                        <xsd:element ref="NumberInParty" />
                        <xsd:element minOccurs="0" name="Information">
                          <xsd:annotation>
                            <xsd:documentation source="DESCRIPTION" xml:lang="en">Other information.</xsd:documentation>
                          </xsd:annotation>
                          <xsd:complexType>
                            <xsd:sequence>
                              <xsd:element maxOccurs="unbounded" name="Text" type="PAXType" />
                            </xsd:sequence>
                          </xsd:complexType>
                        </xsd:element>
                      </xsd:sequence>
                      <xsd:attribute ref="Source" />
                      <xsd:attribute ref="SourceRef" />
                      <xsd:attribute name="FlownIndicator" type="ChoiceType" use="optional">
                        <xsd:annotation>
                          <xsd:documentation source="DESCRIPTION" xml:lang="en">If Y, segment has been flown.
Example: Y (YES)</xsd:documentation>
                        </xsd:annotation>
                      </xsd:attribute>
                      <xsd:attribute ref="MarriedSegment" use="optional" />
                      <xsd:attribute ref="CnxxIndicator" />
                    </xsd:complexType>
                  </xsd:element>
                </xsd:sequence>
                <xsd:attribute ref="DepartureCode" use="required" />
                <xsd:attribute ref="ArrivalCode" use="required" />
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
          <xsd:attribute name="UserInstruction" type="xsd:string" use="optional">
            <xsd:annotation>
              <xsd:documentation source="DESCRIPTION" xml:lang="en">User instructions for required input.

=notes=
1. A client application should use this as a label for the user input box.</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Required" type="ChoiceType" use="optional">
            <xsd:annotation>
              <xsd:documentation source="DESCRIPTION" xml:lang="en">If Y, user input is mandatory.
Example: Y (YES)</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="UserInputResponseType">
    <xsd:annotation>
      <xsd:documentation source="DESCRIPTION" xml:lang="en">User input required for a service in a response message.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="UserText">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">User entry information.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:attribute name="InputID" type="xsd:string" use="required">
            <xsd:annotation>
              <xsd:documentation source="DESCRIPTION" xml:lang="en">Associated backend component.
Examples:
PUTime
DOTime
Address</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="UserInstruction" type="xsd:string" use="optional">
            <xsd:annotation>
              <xsd:documentation source="DESCRIPTION" xml:lang="en">User instructions for required input.

=notes=
1. A client application should use this as a label for the user input box.</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Required" type="ChoiceType" use="optional">
            <xsd:annotation>
              <xsd:documentation source="DESCRIPTION" xml:lang="en">If Y, user input is mandatory.
Example: Y (YES)</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="OptionList" type="OptionListType">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">User selection based on a dropdown menu.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="UserChoices">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">User choice(s) information.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="FlightOptions">
              <xsd:annotation>
                <xsd:documentation source="DESCRIPTION" xml:lang="en">Alternate flights offered with this service, such as stand by flight choices (or reaccommodation flights).</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element maxOccurs="unbounded" name="Flight">
                    <xsd:annotation>
                      <xsd:documentation source="DESCRIPTION" xml:lang="en">Flight information.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                      <xsd:sequence minOccurs="0">
                        <xsd:element minOccurs="0" ref="JourneyTime" />
                        <xsd:element maxOccurs="unbounded" name="Segment">
                          <xsd:annotation>
                            <xsd:documentation source="DESCRPTION" xml:lang="en">Flight segment(s).</xsd:documentation>
                          </xsd:annotation>
                          <xsd:complexType>
                            <xsd:sequence>
                              <xsd:element name="Departure">
                                <xsd:annotation>
                                  <xsd:documentation source="DESCRIPTION" xml:lang="en">Departure information.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                  <xsd:sequence>
                                    <xsd:element ref="AirportCode" />
                                    <xsd:element name="AirportName" type="xsd:string">
                                      <xsd:annotation>
                                        <xsd:documentation source="DESCRIPTION" xml:lang="en">Airport name.
Example: LAGUARDIA AIRPORT</xsd:documentation>
                                      </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Date" type="DateType">
                                      <xsd:annotation>
                                        <xsd:documentation source="DESCRIPTION" xml:lang="en">Departure date.
Example: 2013-01-11</xsd:documentation>
                                      </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Time" type="TimeType">
                                      <xsd:annotation>
                                        <xsd:documentation source="DESCRIPTION" xml:lang="en">Departure time.
Example: 11:32</xsd:documentation>
                                      </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element minOccurs="0" name="Terminal" type="TerminalType">
                                      <xsd:annotation>
                                        <xsd:documentation source="DESCRIPTION" xml:lang="en">Airport terminal.
Example: 2F</xsd:documentation>
                                      </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element minOccurs="0" name="Gate" type="TerminalType">
                                      <xsd:annotation>
                                        <xsd:documentation source="DESCRIPTION" xml:lang="en">Gate.
Example: B14</xsd:documentation>
                                      </xsd:annotation>
                                    </xsd:element>
                                  </xsd:sequence>
                                </xsd:complexType>
                              </xsd:element>
                              <xsd:element name="Arrival">
                                <xsd:annotation>
                                  <xsd:documentation source="DESCRIPTION" xml:lang="en">Arrival information.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                  <xsd:sequence>
                                    <xsd:element ref="AirportCode" />
                                    <xsd:element name="AirportName" type="xsd:string">
                                      <xsd:annotation>
                                        <xsd:documentation source="DESCRIPTION" xml:lang="en">Airport name.
Example: CHARLES DE GAULLE AIRPORT</xsd:documentation>
                                      </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Date" type="DateType">
                                      <xsd:annotation>
                                        <xsd:documentation source="DESCRIPTION" xml:lang="en">Arrival date.
Example: 2013-01-15</xsd:documentation>
                                      </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Time" type="TimeType">
                                      <xsd:annotation>
                                        <xsd:documentation source="DESCRIPTION" xml:lang="en">Arrival time.
Example: 14:30</xsd:documentation>
                                      </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element minOccurs="0" name="ChangeOfDay" type="xsd:integer">
                                      <xsd:annotation>
                                        <xsd:documentation source="DESCRIPTION" xml:lang="en">Number of days between arrival and departure.
Example: 1</xsd:documentation>
                                      </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element minOccurs="0" name="Terminal" type="TerminalType">
                                      <xsd:annotation>
                                        <xsd:documentation source="DESCRIPTION" xml:lang="en">Terminal.
Example: 2F</xsd:documentation>
                                      </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element minOccurs="0" name="Gate" type="TerminalType">
                                      <xsd:annotation>
                                        <xsd:documentation source="DESCRIPTION" xml:lang="en">Gate.
Example: B14</xsd:documentation>
                                      </xsd:annotation>
                                    </xsd:element>
                                  </xsd:sequence>
                                </xsd:complexType>
                              </xsd:element>
                              <xsd:element name="Carrier">
                                <xsd:annotation>
                                  <xsd:documentation source="DESCRIPTION" xml:lang="en">Flight carrier information.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                  <xsd:sequence>
                                    <xsd:element ref="AirlineCode" />
                                    <xsd:element name="AirlineName" type="xsd:string">
                                      <xsd:annotation>
                                        <xsd:documentation source="DESCRIPTION" xml:lang="en">Airline name.
Example: UNITED AIRLINES</xsd:documentation>
                                      </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element ref="FlightNumber" />
                                  </xsd:sequence>
                                </xsd:complexType>
                              </xsd:element>
                              <xsd:element minOccurs="0" name="OperatingCarrier" type="AirlineOperatorCoreType">
                                <xsd:annotation>
                                  <xsd:documentation source="DESCRIPTION" xml:lang="en">Operating carrier information.</xsd:documentation>
                                </xsd:annotation>
                              </xsd:element>
                              <xsd:element name="Equipment" type="EquipmentSummaryType">
                                <xsd:annotation>
                                  <xsd:documentation source="DESCRIPTION" xml:lang="en">Aircraft equipment information.</xsd:documentation>
                                </xsd:annotation>
                              </xsd:element>
                              <xsd:element minOccurs="0" ref="NumberOfStops" />
                              <xsd:element minOccurs="0" maxOccurs="6" name="StopInformation">
                                <xsd:annotation>
                                  <xsd:documentation source="DESCRIPTION" xml:lang="en">Stop information.</xsd:documentation>
                                  <xsd:documentation source="GUIDELINES" xml:lang="en">=notes=
1. A Change of Gauge flight requires the @ChangeOfGauge attribute to be set to Y and new equipment specified from that stop forward.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                  <xsd:sequence>
                                    <xsd:element ref="AirportCode" />
                                    <xsd:element name="AirportName" type="xsd:string">
                                      <xsd:annotation>
                                        <xsd:documentation source="DESCRIPTION" xml:lang="en">Airport name.
Example: CHARLES DE GAULLE AIRPORT</xsd:documentation>
                                      </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="ArrivalDate" type="DateType">
                                      <xsd:annotation>
                                        <xsd:documentation source="DESCRIPTION" xml:lang="en">Arrival date of one or more flights related to specified fare information.
Example: 2013-01-15</xsd:documentation>
                                      </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="ArrivalTime" type="TimeType">
                                      <xsd:annotation>
                                        <xsd:documentation source="DESCRIPTION" xml:lang="en">Stop arrival time.
Example: 14:30</xsd:documentation>
                                      </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="DepartureDate" type="xsd:date">
                                      <xsd:annotation>
                                        <xsd:documentation source="DESCRIPTION" xml:lang="en">Departure date.
Example: 2013-12-25</xsd:documentation>
                                      </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="DepartureTime" type="TimeType">
                                      <xsd:annotation>
                                        <xsd:documentation source="DESCRIPTION" xml:lang="en">Stop departure time.
Example: 14:30</xsd:documentation>
                                      </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element minOccurs="0" name="Equipment" type="EquipmentDetailType">
                                      <xsd:annotation>
                                        <xsd:documentation source="DESCRIPTION" xml:lang="en">Change of gauge aircraft equipment information.</xsd:documentation>
                                        <xsd:documentation source="GUIDELINES" xml:lang="en">=notes=
1. IMPORTANT: ONLY USE THIS in the case of a Change of Gauge flight as the  equipment for the continuation of the flight will be mapped from here.</xsd:documentation>
                                      </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element minOccurs="0" name="GroundTime" type="xsd:integer">
                                      <xsd:annotation>
                                        <xsd:documentation source="DESCRIPTION" xml:lang="en">Aircraft ground time minutes.
Example: 30</xsd:documentation>
                                      </xsd:annotation>
                                    </xsd:element>
                                  </xsd:sequence>
                                  <xsd:attribute name="ChangeOfGauge" type="ChoiceType" use="optional">
                                    <xsd:annotation>
                                      <xsd:documentation source="DESCRIPTION" xml:lang="en">If Y, there has been a Change of Gauge.
Example: Y (YES)</xsd:documentation>
                                      <xsd:documentation source="GUIDELINES" xml:lang="en">=notes=
1. This value should be set to Y if the flight is a Change of Gauge flight.
2. If Y, the new equipment (for the next leg of this flight) must be specified in the Equipment element.</xsd:documentation>
                                    </xsd:annotation>
                                  </xsd:attribute>
                                </xsd:complexType>
                              </xsd:element>
                              <xsd:element minOccurs="0" ref="Meals" />
                              <xsd:element minOccurs="0" ref="FlightDuration" />
                              <xsd:element minOccurs="0" name="LegMiles" type="xsd:string">
                                <xsd:annotation>
                                  <xsd:documentation source="DESCRIPTION" xml:lang="en">Segment mileage quantity.
Example: 2143</xsd:documentation>
                                </xsd:annotation>
                              </xsd:element>
                              <xsd:element minOccurs="0" name="Classes">
                                <xsd:annotation>
                                  <xsd:documentation source="DESCRIPTION" xml:lang="en">Service class information.</xsd:documentation>
                                </xsd:annotation>
                                <xsd:complexType>
                                  <xsd:sequence>
                                    <xsd:element name="ClassOfService">
                                      <xsd:annotation>
                                        <xsd:documentation source="DESCRIPTION" xml:lang="en">Segment booking class of service.
Example: B</xsd:documentation>
                                        <xsd:documentation source="GUIDELINES" xml:lang="en">=notes=
1. This booking class of service will be applied to this segment when booked as part of this FareGroup.</xsd:documentation>
                                      </xsd:annotation>
                                      <xsd:complexType>
                                        <xsd:simpleContent>
                                          <xsd:extension base="FlightCOSType">
                                            <xsd:attribute name="Status" type="AvailSeatStatusType" use="optional">
                                              <xsd:annotation>
                                                <xsd:documentation source="DESCRIPTION" xml:lang="en">Status for available seats.
Example: SEATS ON REQUEST ONLY TO AIRLINE

For maximum interoperability, Refer to IATA PADIS Code List for data element 9865. Select a code value or enter a value that is bilaterally agreed upon between trading partners.</xsd:documentation>
                                              </xsd:annotation>
                                            </xsd:attribute>
                                            <xsd:attribute name="Quantity" type="xsd:positiveInteger" use="optional">
                                              <xsd:annotation>
                                                <xsd:documentation source="DESCRIPTION" xml:lang="en">Quantity of available seats.
Example: 8

=notes=
1. This may be the actual quantity or a range of available seats for this service class.</xsd:documentation>
                                              </xsd:annotation>
                                            </xsd:attribute>
                                            <xsd:attribute ref="PriceClassName" use="optional">
                                              <xsd:annotation>
                                                <xsd:documentation source="DESCRIPTION" xml:lang="en">Associated price class name.
Example: SUPERSAVER</xsd:documentation>
                                              </xsd:annotation>
                                            </xsd:attribute>
                                          </xsd:extension>
                                        </xsd:simpleContent>
                                      </xsd:complexType>
                                    </xsd:element>
                                  </xsd:sequence>
                                </xsd:complexType>
                              </xsd:element>
                            </xsd:sequence>
                            <xsd:attribute name="ChangeOfAirport" type="ChoiceType" use="optional">
                              <xsd:annotation>
                                <xsd:documentation source="DESCRIPTION" xml:lang="en">If Y, there is a change of airport in the connection.
Example: Y (YES)</xsd:documentation>
                              </xsd:annotation>
                            </xsd:attribute>
                            <xsd:attribute ref="MarriedSegment" use="optional" />
                            <xsd:attribute ref="CnxxIndicator" use="optional" />
                            <xsd:attribute name="SecureFlight" type="ChoiceType" use="optional">
                              <xsd:annotation>
                                <xsd:documentation source="DESCRIPTION" xml:lang="en">If Y, secure flight data is required for this flight.
Example: N (NO)</xsd:documentation>
                              </xsd:annotation>
                            </xsd:attribute>
                          </xsd:complexType>
                        </xsd:element>
                      </xsd:sequence>
                    </xsd:complexType>
                  </xsd:element>
                </xsd:sequence>
                <xsd:attribute ref="DepartureCode" use="required" />
                <xsd:attribute ref="ArrivalCode" use="required" />
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
          <xsd:attribute name="UserInstruction" type="xsd:string" use="optional">
            <xsd:annotation>
              <xsd:documentation source="DESCRIPTION" xml:lang="en">User instructions for required input.

=notes=
1. A client application should use this as a label for the user input box.</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
          <xsd:attribute name="Required" type="ChoiceType" use="optional">
            <xsd:annotation>
              <xsd:documentation source="DESCRIPTION" xml:lang="en">If Y, user input is mandatory.
Example: Y (YES)</xsd:documentation>
            </xsd:annotation>
          </xsd:attribute>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="UserInputSrvcType">
    <xsd:annotation>
      <xsd:documentation source="DESCRIPTION" xml:lang="en">Return of user input/selection from POS application.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="UserText">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">User entry information.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:simpleContent>
            <xsd:extension base="xsd:string">
              <xsd:attribute name="InputID" type="xsd:string" use="required">
                <xsd:annotation>
                  <xsd:documentation source="DESCRIPTION" xml:lang="en">Associated backend component.
Examples:
PUTime
DOTime
Address</xsd:documentation>
                </xsd:annotation>
              </xsd:attribute>
            </xsd:extension>
          </xsd:simpleContent>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="OptionList" type="OptionListInputType">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">User selection from the list menu offered.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="UserChoices">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">User selection.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="FlightOptions">
              <xsd:annotation>
                <xsd:documentation source="DESCRIPTION" xml:lang="en">Alternate flights offered with this service, such as stand by flight choices (or reaccommodation flights).</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element maxOccurs="unbounded" name="Flight">
                    <xsd:annotation>
                      <xsd:documentation source="DESCRIPTION" xml:lang="en">Flight information.</xsd:documentation>
                    </xsd:annotation>
                    <xsd:complexType>
                      <xsd:sequence>
                        <xsd:element name="Carrier">
                          <xsd:annotation>
                            <xsd:documentation source="DESCRIPTION" xml:lang="en">Carrier information.</xsd:documentation>
                          </xsd:annotation>
                          <xsd:complexType>
                            <xsd:sequence>
                              <xsd:element ref="AirlineCode" />
                              <xsd:element name="AirlineName" type="xsd:string">
                                <xsd:annotation>
                                  <xsd:documentation source="DESCRIPTION" xml:lang="en">Airline name.
Example: UNITED AIRLINES</xsd:documentation>
                                </xsd:annotation>
                              </xsd:element>
                              <xsd:element minOccurs="0" ref="FlightNumber" />
                            </xsd:sequence>
                          </xsd:complexType>
                        </xsd:element>
                        <xsd:element minOccurs="0" name="OperatingCarrier">
                          <xsd:annotation>
                            <xsd:documentation source="DESCRIPTION" xml:lang="en">Operating carrier information.</xsd:documentation>
                          </xsd:annotation>
                          <xsd:complexType>
                            <xsd:sequence>
                              <xsd:element minOccurs="0" ref="AirlineCode" />
                              <xsd:element name="AirlineName" type="xsd:string">
                                <xsd:annotation>
                                  <xsd:documentation source="DESCRIPTION" xml:lang="en">Airline name.
Example: UNITED AIRLINES</xsd:documentation>
                                </xsd:annotation>
                              </xsd:element>
                              <xsd:element minOccurs="0" ref="FlightNumber" />
                              <xsd:element minOccurs="0" ref="ClassOfService" />
                            </xsd:sequence>
                          </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="Departure">
                          <xsd:annotation>
                            <xsd:documentation source="DESCRIPTION" xml:lang="en">Departure information.</xsd:documentation>
                          </xsd:annotation>
                          <xsd:complexType>
                            <xsd:sequence>
                              <xsd:element ref="AirportCode" />
                              <xsd:element name="AirportName" type="xsd:string">
                                <xsd:annotation>
                                  <xsd:documentation source="DESCRIPTION" xml:lang="en">Airport name.
Example: LAGUARDIA AIRPORT</xsd:documentation>
                                </xsd:annotation>
                              </xsd:element>
                              <xsd:element name="Date" type="DateType">
                                <xsd:annotation>
                                  <xsd:documentation source="DESCRIPTION" xml:lang="en">Departure date.
Example: 2013-01-11</xsd:documentation>
                                </xsd:annotation>
                              </xsd:element>
                              <xsd:element minOccurs="0" name="Time" type="TimeType">
                                <xsd:annotation>
                                  <xsd:documentation source="DESCRIPTION" xml:lang="en">Departure time.
Example: 11:32</xsd:documentation>
                                </xsd:annotation>
                              </xsd:element>
                            </xsd:sequence>
                          </xsd:complexType>
                        </xsd:element>
                        <xsd:element name="Arrival">
                          <xsd:annotation>
                            <xsd:documentation source="DESCRIPTION" xml:lang="en">Arrival information.</xsd:documentation>
                          </xsd:annotation>
                          <xsd:complexType>
                            <xsd:sequence>
                              <xsd:element ref="AirportCode" />
                              <xsd:element name="AirportName" type="xsd:string">
                                <xsd:annotation>
                                  <xsd:documentation source="DESCRIPTION" xml:lang="en">Airport name.
Example: CHARLES DE GAULLE AIRPORT</xsd:documentation>
                                </xsd:annotation>
                              </xsd:element>
                              <xsd:element name="Date" type="DateType">
                                <xsd:annotation>
                                  <xsd:documentation source="DESCRIPTION" xml:lang="en">Arrival date.
Example: 2013-01-15</xsd:documentation>
                                </xsd:annotation>
                              </xsd:element>
                              <xsd:element minOccurs="0" name="Time" type="TimeType">
                                <xsd:annotation>
                                  <xsd:documentation source="DESCRIPTION" xml:lang="en">Arrival time.
Example: 14:30</xsd:documentation>
                                </xsd:annotation>
                              </xsd:element>
                              <xsd:element minOccurs="0" name="ChangeOfDay" type="xsd:integer">
                                <xsd:annotation>
                                  <xsd:documentation source="DESCRIPTION" xml:lang="en">Number of days between arrival and departure.
Example: 1</xsd:documentation>
                                </xsd:annotation>
                              </xsd:element>
                            </xsd:sequence>
                          </xsd:complexType>
                        </xsd:element>
                        <xsd:element minOccurs="0" name="Equipment" type="EquipmentIDType">
                          <xsd:annotation>
                            <xsd:documentation source="DESCRIPTION" xml:lang="en">Aircraft equipment information.</xsd:documentation>
                          </xsd:annotation>
                        </xsd:element>
                        <xsd:element ref="ClassOfService" />
                        <xsd:element minOccurs="0" name="ActionCode">
                          <xsd:annotation>
                            <xsd:documentation source="DESCRIPTION" xml:lang="en">Reservation system action code value.</xsd:documentation>
                          </xsd:annotation>
                          <xsd:complexType>
                            <xsd:simpleContent>
                              <xsd:extension base="xsd:string" />
                            </xsd:simpleContent>
                          </xsd:complexType>
                        </xsd:element>
                      </xsd:sequence>
                      <xsd:attribute ref="Source" />
                    </xsd:complexType>
                  </xsd:element>
                </xsd:sequence>
                <xsd:attribute ref="DepartureCode" use="required" />
                <xsd:attribute ref="ArrivalCode" use="required" />
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="UserText">
    <xsd:annotation>
      <xsd:documentation source="DESCRIPTION" xml:lang="en">User entry information.</xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="xsd:string">
        <xsd:attribute name="InputID" type="xsd:string" use="required">
          <xsd:annotation>
            <xsd:documentation source="DESCRIPTION" xml:lang="en">Associated backend component.
Examples:
PUTime
DOTime
Address</xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:simpleType name="Time">
    <xsd:annotation>
      <xsd:documentation>Time in ISO 8601 twenty-four hour clock format. Example: 14:00</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:length value="5" />
      <xsd:pattern value="([0-1][0-9]|2[0-3]):[0-5][0-9]" />
    </xsd:restriction>
  </xsd:simpleType>
</xsd:schema>