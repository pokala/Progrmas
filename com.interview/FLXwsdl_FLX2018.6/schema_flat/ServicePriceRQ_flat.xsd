                      <xsd:documentation source="DESCRIPTION" xml:lang="en">Aircraft equipment code.
Example: 318</xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                      <xsd:restriction base="xsd:string">
                        <xsd:minLength value="3" />
                        <xsd:maxLength value="4" />
                      </xsd:restriction>
                    </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="Name" type="xsd:string">
                    <xsd:annotation>
                      <xsd:documentation source="DESCRIPTION" xml:lang="en">Aircraft equipment name.
Example: AIRBUS</xsd:documentation>
                    </xsd:annotation>
                  </xsd:element>
                </xsd:sequence>
                <xsd:attribute name="AircraftType" type="xsd:string" use="optional">
                  <xsd:annotation>
                    <xsd:documentation source="DESCRIPTION" xml:lang="en">Airline specific aircraft equipment type code.
Example: 747</xsd:documentation>
                  </xsd:annotation>
                </xsd:attribute>
              </xsd:complexType>
            </xsd:element>
            <xsd:element minOccurs="0" name="UserInput" type="UserInputResponseType">
              <xsd:annotation>
                <xsd:documentation source="DESCRIPTION" xml:lang="en">User input required for this service.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="Endorsement">
        <xsd:annotation>
          <xsd:documentation>Endorsements for this service.

Example: NONREF</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="Text" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>Freeflow text.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
          <xsd:attribute name="Type" use="optional">
            <xsd:annotation>
              <xsd:documentation>Identifies the creation mode of this line. Values are:
A - automatically by pricing system
</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction base="xsd:string">
                <xsd:length value="1" />
                <xsd:enumeration value="A" />
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:attribute>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" ref="Penalties" />
      <xsd:element minOccurs="0" ref="Commission" />
      <xsd:element minOccurs="0" ref="Combinality" />
      <xsd:element minOccurs="0" ref="IncentiveData" />
      <xsd:element minOccurs="0" ref="Diagnostics" />
      <xsd:element minOccurs="0" ref="Attributes" />
      <xsd:element minOccurs="0" ref="MediaReference" />
      <xsd:element minOccurs="0" ref="BookByDate" />
      <xsd:element minOccurs="0" name="WebLinks">
        <xsd:annotation>
          <xsd:documentation>Links to associated promotional material</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element maxOccurs="unbounded" name="WebLink">
              <xsd:complexType>
                <xsd:attribute name="Type">
                  <xsd:annotation>
                    <xsd:documentation>Indicates what the content of the web link is; these are possible values:
“I”  Image
“V” Video
“C” HTML or other webpages</xsd:documentation>
                  </xsd:annotation>
                  <xsd:simpleType>
                    <xsd:restriction base="xsd:string">
                      <xsd:enumeration value="I" />
                      <xsd:enumeration value="V" />
                      <xsd:enumeration value="C" />
                    </xsd:restriction>
                  </xsd:simpleType>
                </xsd:attribute>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Source" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>Content source.
Example: BA</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="ReasonCode" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>IATA Reason for Issuance Code (RFIC) code. 
Example: A (AIR TRANSPORTATION)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="ServiceCode" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>Service code.
Examples:   
XLEG (Extra Leg Room Seat, implementer encoding) 
XBAG (Extra Bag, implementer encoding)     OC (ATPCO encoding)

                                          =notes===                    1. For ATPCO encoding, the default value for this attribute is "OC".</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="SubCode" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>Service reason for issuance subcode.               
Example: 0CC (FIRST CHECKED BAG)

                                  =notes=       
1. For ATPCO service encoding, this is the Reason For Issuance Subcode.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="Method" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>Settlement method. 
Example:                        DS (Direct Settlement)

     For maximum interoperability, Refer to IATA PADIS code list SST - Service Settlement Type. Select a code value or enter a value that is bilaterally agreed upon between trading partners.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="Type" type="xsd:string">
      <xsd:annotation>
        <xsd:documentation>Service fee basis.
Examples:
DIS (Option directly applied to fare)
FEE (Baggage only)
FNA (Item is not available/ applicable)
INC (Item is free of charge)  
NAP (Item not applicable/ available)                      
SUR (Surcharge)

For maximum interoperability,     Refer to IATA PADIS code list SFB - Service Fee Basis. Select a code value or enter a value that is bilaterally agreed upon between trading partners.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="TypeValue" type="xsd:string">
      <xsd:annotation>
        <xsd:documentation>Service coupon type.
Examples:
1 (Standalone)
2 (Flight Associated)
3 (Ticket Associated)
4 (No EMD, Carrier Directed)
5 (E-Ticket)

For maximum interoperability, Refer to IATA PADIS code list SCT - Service Coupon Type. Select a code value or enter a value that is bilaterally agreed upon between trading partners.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="Airline" type="xsd:string">
      <xsd:annotation>
        <xsd:documentation>Airline designator code of service provider.  
Example: UA</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="FareBrand" type="xsd:string">
      <xsd:annotation>
        <xsd:documentation>Fare brand that service is bundled with.
Example: SUPERSAVER</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="ApplyToAll" type="xsd:string">
      <xsd:annotation>
        <xsd:documentation>If Y, this offer, if selected, applies to all passengers in the PNR.
Example:                          Y (YES)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="CanDeselect" type="xsd:string">
      <xsd:annotation>
        <xsd:documentation>If Y, this service item can be declined and DESELECTED, e.g. an alcoholic beverage coupon.
Example:                          Y (YES)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="Bundle" type="xsd:string">
      <xsd:annotation>
        <xsd:documentation>If Y, this offer is part of a BUNDLED OFFER.
Example:                            Y (YES)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="CanUnbundle" type="xsd:string">
      <xsd:annotation>
        <xsd:documentation>If Y, service can be UNBUNDLED from a service bundle.
Example:                          Y (YES)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="NotValidBefore" type="xsd:dateTime">
      <xsd:annotation>
        <xsd:documentation>The EARLIEST date the option is available or may be used.
Example: 2013-01-13</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="NotValidAfter" type="xsd:dateTime">
      <xsd:annotation>
        <xsd:documentation>The LATEST date the option is available or may be used.
Example: 2014-01-13</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="Icon" type="xsd:string">
      <xsd:annotation>
        <xsd:documentation>Service marketing icon name or ID. Example: MEALS</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="DisplayPriority" type="xsd:string">
      <xsd:annotation>
        <xsd:documentation>Display priority.
Example: S (SECONDARY DISPLAY)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="MaxQuantity" type="xsd:positiveInteger">
      <xsd:annotation>
        <xsd:documentation>Maximum item/ service ordering quantity.
Example: 5</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="DealID" type="xsd:string">
      <xsd:annotation>
        <xsd:documentation>FQTV promotion ID.
Example: GOBIG</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="FQTVCarrier">
      <xsd:annotation>
        <xsd:documentation>Frequent flier program carrier owner.
Example: UA</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:minLength value="2" />
          <xsd:maxLength value="3" />
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
    <xsd:attribute name="FQTVStatus" type="xsd:string">
      <xsd:annotation>
        <xsd:documentation>Traveler FQTV status or tier level.
Example: PLATINUM</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="FQTVNumber" type="xsd:string">
      <xsd:annotation>
        <xsd:documentation>Traveler frequent flyer number.
Example: 6221H3</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="ProviderIDRef" type="xsd:string">
      <xsd:annotation>
        <xsd:documentation>Actual service provider or fulfillment source reference.
Example: FOODSRVCORP</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="Status" type="xsd:string">
      <xsd:annotation>
        <xsd:documentation>Service processing/ settlement status.
Examples:
NN (REQUESTED)
HK (CONFIRMED)</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="TravelerEligible" type="xsd:string">
      <xsd:annotation>
        <xsd:documentation>Indicates if the traveler is eligible for the service priced. Values: "Y" or "N" If not present defaulted to "N"</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="ServiceItemRefType">
    <xsd:annotation>
      <xsd:documentation source="DESCRIPTION" xml:lang="en">Core PRICED ancillary service item with segment and traveler reference definition.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent mixed="false">
      <xsd:extension base="ServiceItemCoreType">
        <xsd:sequence>
          <xsd:element minOccurs="0" maxOccurs="unbounded" ref="SegmentIDRef" />
          <xsd:element minOccurs="0" ref="TravelerIDRef" />
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="ServiceItemNoPriceType">
    <xsd:annotation>
      <xsd:documentation source="DESCRIPTION" xml:lang="en">Core NON-PRICED ancillary service item definition.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" ref="ServiceID">
        <xsd:annotation>
          <xsd:documentation>Unique (non-priced) Service ID.
Example: C001

=notes=
1. This is a unique ID provided by the airline for a non-priced ancillary service that may be part of an Ancillary Service Bundle.

2. An additional OfferID may be specified in the ServicePrice element if this is a priced offer. Note that if this is a priced offer, the ServiceID specified here becomes part of the Priced Offer.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" ref="ForInfo" />
      <xsd:element name="Description" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Commercial name (title description) of offered service.
Example: 3RD BAG FREE</xsd:documentation>
          <xsd:documentation source="ENCODING" xml:lang="en">ATPCO ENCODING: Note, ATPCO S5 Commercial Name.</xsd:documentation>
          <xsd:documentation source="GUIDELINES" xml:lang="en">=notes=
1. UI/ display tip: You may build a one line description to display by stringing the following values together:
"Description"+"ServiceLocation"+"ServiceDate"+"DescriptionVariable" which could be displayed as sampled here:
'Airline Lounge Pass DFW 10May' - where 'Airline Lounge Pass' is the "Description", 'DFW' the "ServiceLocation" and '10May' the "ServiceDate"
-or-
Premium Seat 9D - where 'Premium Seat' is the "Description" and '9D' the "DescriptionVariable"</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" ref="DescriptionVariable" />
      <xsd:element minOccurs="0" ref="DescriptionDetails" />
      <xsd:element minOccurs="0" ref="BundleContent" />
      <xsd:element minOccurs="0" name="Amount" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Currency display price for this option or option differential.
Example: 25.50</xsd:documentation>
          <xsd:documentation source="ENCODING" xml:lang="en">=accepted values=
A string value that includes the price as a major and minor unit, e.g. dollar amount with a decimal and cents.</xsd:documentation>
          <xsd:documentation source="GUIDELINES" xml:lang="en">=notes=
1. This is for display purposes only and not associated to a currency code.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="RedemptionAmount" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Award redemption quantity display price for this option or option differential.
Example: 15000 MILES</xsd:documentation>
          <xsd:documentation source="GUIDELINES" xml:lang="en">=notes=
1. This is for display purposes only.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" ref="ServiceLocation" />
      <xsd:element minOccurs="0" ref="TicketByDate" />
      <xsd:element minOccurs="0" ref="BookByDate" />
      <xsd:element minOccurs="0" ref="ServiceDate" />
      <xsd:element minOccurs="0" ref="ValidatingCarrier" />
      <xsd:element minOccurs="0" ref="InstantPurchase" />
      <xsd:element minOccurs="0" ref="Disclosure" />
      <xsd:element minOccurs="0" name="BookingInstructions">
        <xsd:annotation>
          <xsd:documentation source="DESCRIPTION" xml:lang="en">Optional service booking instructions.</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="0" maxOccurs="unbounded" name="SSRCode" type="SSRCodeType">
              <xsd:annotation>
                <xsd:documentation source="DESCRIPTION" xml:lang="en">Passenger assistance SSR code value.
Examples:
EXST (EXTRA SEAT REQUIRED)
INFT (INFANT ACCOMPANYING ADULT PASSENGER)
PETC (PASSENGER TRAVELING WITH PET IN CABIN)
UMNR (UNACCOMPANIED MINOR)

=notes=
1. Although optional in the schema, some SSR codes may require associated description elements.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" maxOccurs="unbounded" name="OSIText" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation source="DESCRIPTION" xml:lang="en">Other service information (OSI) text.
Example: PAX VIP MEMBER OF PARLIAMENT</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Method" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation source="DESCRIPTION" xml:lang="en">PNR booking method.
Example: AE (BY SSR - SPECIAL SERVICE REQUEST)

For maximum interoperability, refer to IATA PADIS Code List for data element 9750 - Option. Select a code value or enter a value that is bilaterally agreed upon between trading partners.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="UpgradeMethod">
              <xsd:annotation>
                <xsd:documentation source="DESCRIPTION" xml:lang="en">Upgrade method.
Example: AE (BY SSR - SPECIAL SERVICE REQUEST)

For maximum interoperability, refer to IATA PADIS Code List for data element 9750 - Option. Select a code value or enter a value that is bilaterally agreed upon between trading partners.</xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:simpleContent>
                  <xsd:extension base="xsd:string">
                    <xsd:attribute name="NewClass" type="FlightCOSType" use="optional">
                      <xsd:annotation>
                        <xsd:documentation source="DESCRIPTION" xml:lang="en">New class of service.
Example: B</xsd:documentation>
                      </xsd:annotation>
                    </xsd:attribute>
                  </xsd:extension>
                </xsd:simpleContent>
              </xsd:complexType>
            </xsd:element>
            <xsd:element minOccurs="0" maxOccurs="unbounded" name="Text" type="FreeFormTextType">
              <xsd:annotation>
                <xsd:documentation source="DESCRIPTION" xml:lang="en">Data center instructions.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="Equipment" type="EquipmentDetailType">
              <xsd:annotation>
                <xsd:documentation source="DESCRIPTION" xml:lang="en">Aircraft equipment information.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="UserInput" type="UserInputResponseType">
              <xsd:annotation>
                <xsd:documentation source="DESCRIPTION" xml:lang="en">User input required for this service.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="Endorsement">
        <xsd:annotation>
          <xsd:documentation>Endorsements for this service.

Example: NONREF</xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="Text" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>Freeflow text.</xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
          <xsd:attribute name="Type" use="optional">
            <xsd:annotation>
              <xsd:documentation>Identifies the creation mode of this line. Values are:
A - automatically by pricing system
</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
              <xsd:restriction base="xsd:string">
                <xsd:length value="1" />
                <xsd:enumeration value="A" />
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:attribute>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" ref="Penalties" />
      <xsd:element minOccurs="0" ref="Commission" />
      <xsd:element minOccurs="0" ref="Combinality" />
      <xsd:element minOccurs="0" ref="IncentiveData" />
      <xsd:element minOccurs="0" ref="Diagnostics" />
      <xsd:element minOccurs="0" ref="Attributes" />
      <xsd:element minOccurs="0" ref="MediaReference" />
    </xsd:sequence>
    <xsd:attributeGroup ref="SrvcItemAttributesGroup" />
  </xsd:complexType>
